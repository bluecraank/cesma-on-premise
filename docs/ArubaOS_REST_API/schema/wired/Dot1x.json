{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Global 802.1X configuration data.", 
    "title": "Dot1x", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "is_dot1x_enabled":
        {
            "description": "Global 802.1X admin status.", 
            "type": "boolean"
        }, 
        "cached_reauth_delay":
        {
            "description": "Global 802.1X Cached reauth delay.", 
            "minimum": 0, 
            "maximum": 2147483647, 
            "type": "integer", 
            "default_value": 0
        }, 
        "allow_gvrp_vlans":
        {
            "description": "Allow GVRP Vlans", 
            "type": "boolean"
        }, 
        "use_lldp_data":
        {
            "description": "use LLDP data", 
            "type": "boolean"
        }
    }
}
