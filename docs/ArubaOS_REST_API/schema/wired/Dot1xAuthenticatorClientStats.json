{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Dot1x authenticator client session statistics", 
    "title": "Dot1xAuthenticatorClientStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID", 
            "type": "string", 
            "maxLength": 9, 
            "static_url": ["/system/status/switch#/blades/data_ports/port_name"], 
            "dynamic_url": ["/system/status/switch#/blades/expansion_modules/data_ports/port_name"]
        }, 
        "client_name":
        {
            "description": "Client Name", 
            "type": "string"
        }, 
        "mac_address":
        {
            "description": "Mac Address", 
            "type": "string"
        }, 
        "ip_address":
        {
            "description": "IP Address", 
            "type": "string"
        }, 
        "client_status":
        {
            "description": "Client status", 
            "type": "string"
        }
    }
}
