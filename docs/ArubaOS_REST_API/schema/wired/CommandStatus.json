{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "List the filename along with line numbers and commands, that were failed to be added/removed during cfg-restore process.", 
    "title": "CommandStatus", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "line_number":
        {
            "description": "Line number.", 
            "type": "string"
        }, 
        "command":
        {
            "description": "Failed command.", 
            "type": "string"
        }, 
        "failure_reason":
        {
            "description": "Failure Reason.", 
            "type": "string"
        }
    }
}
