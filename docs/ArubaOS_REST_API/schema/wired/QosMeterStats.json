{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Qos Meter Status", 
    "title": "QosMeterStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "traffic_class_id":
        {
            "description": "QoS Traffic Class Id", 
            "type": "string"
        }, 
        "meter_stats":
        {
            "description": "Meter Stats for the Class which is assigned Rate Limit Action", 
            "type": "integer"
        }
    }
}
