{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "SNMP Server Response Source Settings", 
    "title": "SnmpServerResponseSource", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "rs_ip_policy":
        {
            "description": "The information about the destination of response.", 
            "$ref":"../../schema/wired/ResponseSourceIPPolicy.json", 
            "default_value": "RSIP_DEFAULT", 
            "sql.not_null": true
        }, 
        "loopback_interface":
        {
            "description": "Loopback interface number in case the Mode is loopback.", 
            "minimum": 0, 
            "maximum": 7, 
            "type": "integer"
        }, 
        "ip_addr":
        {
            "description": "IP address in case Mode is specified as IP.", 
            "$ref":"../../schema/common/IpAddress.json"
        }
    }
}
