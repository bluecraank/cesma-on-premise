{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Creating a certificate signing request details.", 
    "title": "CsrConfigDetail", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "certificate_name":
        {
            "description": "Certificate Name", 
            "type": "string", 
            "maxLength": 20, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "ta_profile":
        {
            "description": "Ta profile", 
            "type": "string", 
            "maxLength": 100, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "key_type":
        {
            "description": "Key_type details.", 
            "$ref":"../../schema/wired/CsrKeyType.json"
        }, 
        "subject":
        {
            "description": "Subject details.", 
            "$ref":"../../schema/wired/CsrSubjectDetails.json", 
            "sql.not_null": true
        }, 
        "cert_usage":
        {
            "description": "Intended usage of Certificate.", 
            "$ref":"../../schema/wired/CsrApplications.json", 
            "default_value": "CA_WEB"
        }, 
        "validity":
        {
            "description": "Validity details.", 
            "$ref":"../../schema/wired/CsrValidityDates.json"
        }
    }
}
