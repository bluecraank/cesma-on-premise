{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Wraps the parameters to execute chassislocate command.", 
    "title": "LocatorLedBlinkCommand", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "led_blink_status":
        {
            "description": "LED status (blink/on/off)", 
            "$ref":"../../schema/wired/LedStatus.json", 
            "sql.not_null": true
        }, 
        "duration_in_minutes":
        {
            "description": "Duration in minutes", 
            "minimum": 1, 
            "maximum": 1440, 
            "type": "integer", 
            "default_value": 30
        }, 
        "when":
        {
            "description": "When to blink/on the LED.", 
            "$ref":"../../schema/wired/LedBlinkTime.json"
        }, 
        "member_id":
        {
            "description": "Stacking Member Id. If member_id is not specified, command will be applied to all members", 
            "minimum": 1, 
            "maximum": 4, 
            "type": "integer"
        }
    }
}
