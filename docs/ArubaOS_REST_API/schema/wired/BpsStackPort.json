{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Stacking Ports of the Stack Members", 
    "title": "BpsStackPort", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "member_id":
        {
            "description": "Stack Member", 
            "type": "integer"
        }, 
        "stacking_port":
        {
            "description": "Stacking Port", 
            "type": "integer"
        }, 
        "port_state":
        {
            "description": "Port State", 
            "$ref":"../../schema/wired/StackPortState.json"
        }, 
        "peer_member":
        {
            "description": "Peer Member", 
            "type": "integer"
        }, 
        "peer_port":
        {
            "description": "Peer Port", 
            "type": "integer"
        }
    }
}
