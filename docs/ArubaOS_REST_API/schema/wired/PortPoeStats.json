{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Port PoE statistics data.", 
    "title": "PortPoeStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID.", 
            "type": "string", 
            "maxLength": 9
        }, 
        "poe_detection_status":
        {
            "description": "PoE Detection status", 
            "$ref":"../../schema/wired/PortPoeDetectionStatus.json"
        }, 
        "port_voltage_in_volts":
        {
            "description": "Total voltage being delivered.", 
            "type": "integer"
        }, 
        "power_denied_count":
        {
            "description": "Power denied count", 
            "type": "integer"
        }, 
        "over_current_count":
        {
            "description": "Over Current Count", 
            "type": "integer"
        }, 
        "mps_absent_count":
        {
            "description": "MPS absent count", 
            "type": "integer"
        }, 
        "short_count":
        {
            "description": "Short count", 
            "type": "integer"
        }, 
        "actual_power_in_watts":
        {
            "description": "Actual power value", 
            "type": "integer"
        }, 
        "power_class":
        {
            "description": "Allocated POE Power Class", 
            "type": "integer"
        }
    }
}
