{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "OOBM configuration data.", 
    "title": "Oobm", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "is_oobm_enabled":
        {
            "description": "Global OOBM status.", 
            "type": "boolean", 
            "default_value": true, 
            "sql.not_null": true
        }
    }
}
