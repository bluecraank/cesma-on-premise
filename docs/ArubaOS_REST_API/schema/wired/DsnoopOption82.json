{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "DHCP Snooping Option 82", 
    "title": "DsnoopOption82", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "is_dsnoop_option82_enabled":
        {
            "description": "Whether DHCP Snooping option 82 is enabled", 
            "type": "boolean", 
            "default_value": true
        }, 
        "remote_id":
        {
            "description": "Remote Id Format", 
            "$ref":"../../schema/wired/DsnoopRemoteId.json", 
            "default_value": "DRI_MAC"
        }, 
        "untrusted_policy":
        {
            "description": "Policy for DHCP packets received on untrusted ports that contain option 82", 
            "$ref":"../../schema/wired/DsnoopUntrustedPolicy.json", 
            "default_value": "DUP_DROP"
        }
    }
}
