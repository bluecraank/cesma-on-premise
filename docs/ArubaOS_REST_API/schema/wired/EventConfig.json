{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Config", 
    "title": "EventConfig", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "config_log_msg":
        {
            "description": "Config Log Message", 
            "type": "string"
        }, 
        "offending_string":
        {
            "description": "Config Offending String", 
            "type": "string"
        }, 
        "seq_num":
        {
            "description": "Sequence Number", 
            "type": "integer"
        }, 
        "config_note":
        {
            "description": "Config Note", 
            "type": "string"
        }
    }
}
