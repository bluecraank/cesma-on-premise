{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "QoS Port Policy configuration data.", 
    "title": "QosPortPolicy", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "The Port ID.", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "static_url": ["/system/status/switch#/blades/data_ports/port_name"], 
            "dynamic_url": ["/system/status/switch#/blades/expansion_modules/data_ports/port_name"], 
            "sql.not_null": true
        }, 
        "policy_id":
        {
            "description": "The QoS Policy ID.", 
            "type": "string", 
            "maxLength": 72, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "direction":
        {
            "description": "The QoS Policy Port Direction.", 
            "$ref":"../../schema/wired/QosPortPolicyDirection.json", 
            "sql.not_null": true
        }
    }
}
