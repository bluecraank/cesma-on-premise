{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "MSTP Global Stats", 
    "title": "MstpStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "root_mac_address":
        {
            "description": "MAC address of the Root Switch", 
            "$ref":"../../schema/common/MacAddress.json"
        }, 
        "root_switch_priority":
        {
            "description": "Priority of the Root Switch", 
            "type": "integer"
        }, 
        "root_mstp_port_id":
        {
            "description": "Root Port", 
            "type": "string"
        }, 
        "root_path_cost":
        {
            "description": "Root Path Cost", 
            "type": "integer"
        }
    }
}
