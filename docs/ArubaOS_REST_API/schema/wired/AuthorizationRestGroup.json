{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Authorization Rest Group", 
    "title": "AuthorizationRestGroup", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "group_name":
        {
            "description": "Rest-Uri Authorization group name", 
            "type": "string", 
            "maxLength": 24, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "seq_num":
        {
            "description": "Rest-Uri Authorization sequence number", 
            "minimum": 1, 
            "maximum": 2147483647, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "match_uri":
        {
            "description": "Match URI to authorize", 
            "type": "string", 
            "maxLength": 247, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "match_json":
        {
            "description": "Json payload to match", 
            "type": "string", 
            "maxLength": 1200, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "uri_access":
        {
            "description": "Specify uri-access method to authorize GET/PUT/DELETE/POST", 
            "$ref":"../../schema/wired/HttpAccessMethod.json", 
            "sql.not_null": true
        }, 
        "uri_permission":
        {
            "description": "Match command action Permit/deny", 
            "$ref":"../../schema/wired/AuthorizationPermission.json", 
            "default_value": "AZP_PERMIT", 
            "sql.not_null": true
        }, 
        "is_log_enabled":
        {
            "description": "Action routine to log the matched command", 
            "type": "boolean"
        }, 
        "privilege_level":
        {
            "description": "authorization group privilege level", 
            "type": "integer", 
            "readonly": true, 
            "sql.auto_generated": true
        }
    }
}
