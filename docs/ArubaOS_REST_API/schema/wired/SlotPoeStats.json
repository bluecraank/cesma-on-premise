{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Slot PoE statistics data.", 
    "title": "SlotPoeStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "slot_name":
        {
            "description": "Slot ID.", 
            "type": "string"
        }, 
        "maximum_power_in_watts":
        {
            "description": "Maximum power.", 
            "type": "integer"
        }, 
        "power_in_use_in_watts":
        {
            "description": "Power in use.", 
            "type": "string"
        }, 
        "is_slot_operational":
        {
            "description": "Operational status.", 
            "type": "boolean"
        }
    }
}
