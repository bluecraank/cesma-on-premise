{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Filtering events using event id", 
    "title": "EventIdFilter", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "whitelist":
        {
            "description": "Filter in device events", 
            "$ref":"../../schema/wired/EventIdList.json"
        }, 
        "blacklist":
        {
            "description": "Filter out device events", 
            "$ref":"../../schema/wired/EventIdList.json"
        }
    }
}
