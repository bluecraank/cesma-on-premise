{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Schedule Date and Time for IP-SLA", 
    "title": "IpSlaScheduleTime", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "month":
        {
            "description": "Month (MM)", 
            "minimum": 0, 
            "maximum": 12, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "date":
        {
            "description": "Date (DD)", 
            "minimum": 0, 
            "maximum": 31, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "year":
        {
            "description": "Year (YY)", 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "hours":
        {
            "description": "Hours (HH)", 
            "minimum": 0, 
            "maximum": 23, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "minutes":
        {
            "description": "Minutes (MM)", 
            "minimum": 0, 
            "maximum": 59, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "seconds":
        {
            "description": "Seconds (SS)", 
            "minimum": 0, 
            "maximum": 59, 
            "type": "integer", 
            "sql.not_null": true
        }
    }
}
