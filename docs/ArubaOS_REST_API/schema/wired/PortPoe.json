{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Port PoE configuration data.", 
    "title": "PortPoe", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID.", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "static_url": ["/system/status/switch#/blades/data_ports/port_name"], 
            "dynamic_url": ["/system/status/switch#/blades/expansion_modules/data_ports/port_name"]
        }, 
        "is_poe_enabled":
        {
            "description": "Port PoE status", 
            "type": "boolean", 
            "default_value": true
        }, 
        "poe_priority":
        {
            "description": "Port PoE priority", 
            "$ref":"../../schema/wired/PortPoePriority.json", 
            "default_value": "PPP_LOW"
        }, 
        "poe_allocation_method":
        {
            "description": "PoE allocation method", 
            "$ref":"../../schema/wired/PortPoeAllocationMethod.json", 
            "default_value": "PPAM_USAGE"
        }, 
        "allocated_power_in_watts":
        {
            "description": "Allocated power value. Default value for this param is platform dependent.", 
            "minimum": 1, 
            "maximum": 33, 
            "type": "integer"
        }, 
        "port_configured_type":
        {
            "description": "Port configured type", 
            "type": "string", 
            "maxLength": 256, 
            "minLength": 0
        }, 
        "pre_standard_detect_enabled":
        {
            "description": "pre_std_detect enabled/disable", 
            "type": "boolean"
        }
    }
}
