{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "ACL Port Policy configuration data.", 
    "title": "AclPortPolicy", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "The unique ID of the Port.", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "static_url": ["/system/status/switch#/blades/data_ports/port_name"], 
            "dynamic_url": ["/system/status/switch#/blades/expansion_modules/data_ports/port_name"], 
            "sql.not_null": true
        }, 
        "acl_id":
        {
            "description": "The ACL id used to reference it.", 
            "type": "string", 
            "maxLength": 90, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "direction":
        {
            "description": "Direction for which the rule is applied. Direction AD_CRF is applicable only for acl_type AT_CONNECTION_RATE_FILTER.", 
            "$ref":"../../schema/common/AclDirection.json", 
            "sql.not_null": true
        }
    }
}
