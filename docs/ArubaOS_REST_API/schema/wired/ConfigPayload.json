{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "cfg-backup and patch-file download using payload.", 
    "title": "ConfigPayload", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "config_base64_encoded":
        {
            "description": "Base64 encoded configuration.", 
            "type": "string", 
            "sql.not_null": true
        }, 
        "payload_type":
        {
            "description": "To differentiate between patch file and backup file.", 
            "$ref":"../../schema/wired/RestorePayloadType.json", 
            "sql.not_null": true
        }
    }
}
