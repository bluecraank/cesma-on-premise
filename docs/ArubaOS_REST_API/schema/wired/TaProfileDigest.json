{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "TA Profile Digest Details", 
    "title": "TaProfileDigest", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "ta_name":
        {
            "description": "Configured Trust Anchor profile.", 
            "type": "string", 
            "maxLength": 100, 
            "minLength": 1, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "ta_profile_md5":
        {
            "description": "Message Digest of TA Profile.", 
            "type": "string", 
            "maxLength": 32, 
            "minLength": 0, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "ta_status":
        {
            "description": "Certificate installation status of TA Profile.", 
            "$ref":"../../schema/wired/TaCertificateStatus.json", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "tag_name":
        {
            "description": "Tag-name associated with TA Profile.", 
            "type": "string", 
            "maxLength": 100, 
            "minLength": 1, 
            "readonly": true, 
            "sql.auto_generated": true
        }
    }
}
