{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Dot1x Port Security", 
    "title": "Dot1xPortSecurity", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID.", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "controlled_direction":
        {
            "description": "Traffic controlled direction", 
            "$ref":"../../schema/wired/Dot1xControlledDirection.json", 
            "default_value": "DCD_BOTH"
        }, 
        "allow_mbv":
        {
            "description": "Configuration of MAC Based vlans", 
            "type": "boolean", 
            "default_value": true
        }, 
        "allowed_mixed_users":
        {
            "description": "Allowed users", 
            "type": "boolean"
        }, 
        "is_port_speed_vsa_enabled":
        {
            "description": "Is port speed VSA enabled", 
            "type": "boolean"
        }
    }
}
