{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Global VSF Configuration", 
    "title": "VsfStackInfo", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "domain_id":
        {
            "description": "VSF Domain. Max value allowed is 4294967295.", 
            "type": "integer", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "mac_address":
        {
            "description": "VSF MAC Address", 
            "$ref":"../../schema/common/MacAddress.json", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "topology":
        {
            "description": "VSF Topology", 
            "$ref":"../../schema/wired/StackTopology.json", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "status":
        {
            "description": "VSF Status", 
            "$ref":"../../schema/wired/StackStatus.json", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "uptime":
        {
            "description": "VSF uptime", 
            "$ref":"../../schema/wired/UpTime.json", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "port_speed":
        {
            "description": "VSF Port Speed in GB", 
            "$ref":"../../schema/wired/PortSpeed.json", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "software_version":
        {
            "description": "VSF Software Version", 
            "type": "string", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "name":
        {
            "description": "The system name.", 
            "type": "string", 
            "maxLength": 30, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "location":
        {
            "description": "Location where the system is installed.", 
            "type": "string", 
            "maxLength": 255, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "contact":
        {
            "description": "Contact information for the system.", 
            "type": "string", 
            "maxLength": 255, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "stack_id":
        {
            "description": "VSF stack-id", 
            "type": "string", 
            "maxLength": 32, 
            "readonly": true, 
            "sql.auto_generated": true
        }
    }
}
