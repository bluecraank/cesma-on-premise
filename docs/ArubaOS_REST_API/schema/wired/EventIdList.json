{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Event id list", 
    "title": "EventIdList", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "id":
        {
            "description": "Device event id", 
            "type": "array", 
            "maxItems": 50, 
            "items":
            {
                "$ref": "../../schema/wired/DeviceEventType.json"
            }
        }, 
        "range":
        {
            "description": "Device event id range", 
            "type": "array", 
            "maxItems": 20, 
            "items":
            {
                "$ref":"../../schema/wired/EventIDRange.json"
            }
        }
    }
}
