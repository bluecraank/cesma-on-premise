{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Subscription acknowledgement for the subscription request received.", 
    "title": "LiveMonitoringSubscriptionAck", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "subscription_id":
        {
            "description": "Subscription ID - same as received in subscription request.", 
            "type": "string", 
            "maxLength": 64, 
            "readonly": true, 
            "sql.primary_key": true
        }, 
        "sub_ack_code":
        {
            "description": "Subscription acknowledgement code.", 
            "$ref":"../../schema/wired/SubscribeAckCode.json", 
            "sql.not_null": true
        }
    }
}
