{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Auth Priority", 
    "title": "AuthPriority", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "static_url": ["/system/status/switch#/blades/data_ports/port_name"], 
            "dynamic_url": ["/system/status/switch#/blades/expansion_modules/data_ports/port_name"], 
            "sql.not_null": true
        }, 
        "primary_auth_priority":
        {
            "description": "Primary Auth-Priority", 
            "$ref":"../../schema/wired/AuthOrderPriority.json", 
            "sql.not_null": true
        }, 
        "secondary_auth_priority":
        {
            "description": "Secondary Auth-Priority", 
            "$ref":"../../schema/wired/AuthOrderPriority.json", 
            "sql.not_null": true
        }
    }
}
