{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Port Mirror Session configuration data.", 
    "title": "PortMirrorSession", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "session_id":
        {
            "description": "Mirror session ID.", 
            "minimum": 1, 
            "maximum": 4, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "source_port_id":
        {
            "description": "Source ports associated with the mirror session.", 
            "type": "array", 
            "items":
            {
                "type": "string"
            }, 
            "sql.not_null": true
        }, 
        "destination_port_id":
        {
            "description": "Destination port associated with the mirror session.", 
            "type": "string", 
            "maxLength": 9, 
            "sql.not_null": true
        }
    }
}
