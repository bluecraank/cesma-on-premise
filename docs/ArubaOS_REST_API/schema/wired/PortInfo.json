{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Port Information.", 
    "title": "PortInfo", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port Id.", 
            "type": "integer"
        }, 
        "port_name":
        {
            "description": "Port Name.", 
            "type": "string", 
            "maxLength": 9
        }, 
        "type":
        {
            "description": "Port type.", 
            "$ref":"../../schema/wired/PortType.json"
        }, 
        "adminStatus":
        {
            "description": "Port Admin Status.", 
            "$ref":"../../schema/common/InterfaceAdminStatus.json"
        }, 
        "operStatus":
        {
            "description": "Port Operational Status.", 
            "$ref":"../../schema/common/InterfaceOperStatus.json"
        }, 
        "sub_type":
        {
            "description": "Port Sub Type.", 
            "type": "string"
        }, 
        "mode":
        {
            "description": "Port Config Mode.", 
            "$ref":"../../schema/common/PortConfigMode.json"
        }, 
        "poe_status":
        {
            "description": "PoE Status.", 
            "$ref":"../../schema/wired/PoeStatus.json"
        }, 
        "is_internal":
        {
            "description": "Is Internal/Reserved Port.", 
            "type": "boolean"
        }, 
        "alignment":
        {
            "description": "Port Alignment on the switch.", 
            "$ref":"../../schema/wired/PortAlignment.json"
        }, 
        "is_smartrate":
        {
            "description": "Whether the port is Smartrate capable", 
            "type": "boolean"
        }, 
        "oper_mode":
        {
            "description": "Port Operational Mode", 
            "$ref":"../../schema/wired/OperMode.json"
        }
    }
}
