{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "RADIUS statistics data.", 
    "title": "RadiusStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "ip_address":
        {
            "description": "The IP Address of the RADIUS Server.", 
            "$ref":"../../schema/common/IpAddress.json"
        }, 
        "accounting_stats":
        {
            "description": "The Accounting Stats of the RADIUS Server.", 
            "$ref":"../../schema/wired/AccountingStats.json"
        }, 
        "authentication_stats":
        {
            "description": "The Authentication Stats of the RADIUS Server.", 
            "$ref":"../../schema/wired/AuthenticationStats.json"
        }
    }
}
