{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "VSF LLDP MAD Configuration", 
    "title": "VsfLldpMadConfig", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "mad_ip":
        {
            "description": "The IPv4 address of the MAD device", 
            "$ref":"../../schema/common/IpAddress.json", 
            "sql.not_null": true
        }, 
        "community_name":
        {
            "description": "SNMP community string for the MAD device", 
            "type": "string", 
            "maxLength": 32, 
            "minLength": 1, 
            "sql.not_null": true
        }
    }
}
