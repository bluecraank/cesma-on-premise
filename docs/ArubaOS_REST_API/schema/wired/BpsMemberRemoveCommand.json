{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Erases the stack member's configuration.", 
    "title": "BpsMemberRemoveCommand", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "member_id":
        {
            "description": "BPS Member ID", 
            "minimum": 1, 
            "maximum": 10, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "reboot":
        {
            "description": "Reboot member after removing. Input true for reboot", 
            "type": "boolean"
        }
    }
}
