{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "DHCP Relay Options for Option 82", 
    "title": "DhcpRelayOption82", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "is_option82_enabled":
        {
            "description": "DHCP Relay is enabled or disabled. If input is false all dhcp-relay option 82 parameters will be set to default values. If input is true, then other parameters will get configured.", 
            "type": "boolean", 
            "default_value": false, 
            "sql.not_null": true
        }, 
        "dhcp_relay_action":
        {
            "description": "DHCP Relay action for Option 82", 
            "$ref":"../../schema/wired/DhcpRelayOption82Action.json", 
            "default_value": "DRO82A_REPLACE"
        }, 
        "dhcp_relay_remote_id":
        {
            "description": "DHCP Relay remote id for Option 82", 
            "$ref":"../../schema/wired/DhcpRelayOption82RemoteId.json", 
            "default_value": "DRO82RI_MAC"
        }, 
        "is_validation_enabled":
        {
            "description": "Whether packet validation is enabled/disabled", 
            "type": "boolean"
        }
    }
}
