{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Spanning tree global configuration.", 
    "title": "SpanningTree", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "is_enabled":
        {
            "description": "Enable or Disable spanning tree.", 
            "type": "boolean"
        }, 
        "priority":
        {
            "description": "Priority. Not applicable in RPVST mode.", 
            "minimum": 0, 
            "maximum": 15, 
            "type": "integer", 
            "default_value": 8
        }, 
        "mode":
        {
            "description": "Spanning tree mode.", 
            "$ref":"../../schema/wired/SpanningTreeMode.json", 
            "default_value": "STM_MSTP"
        }
    }
}
