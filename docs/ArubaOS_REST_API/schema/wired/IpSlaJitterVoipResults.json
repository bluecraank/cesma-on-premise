{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "IP-SLA UDP Jitter/ UDP Jitter For Voip results", 
    "title": "IpSlaJitterVoipResults", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "ipsla_id":
        {
            "description": "IP-SLA ID", 
            "type": "integer"
        }, 
        "ipsla_type":
        {
            "description": "IP-SLA Type", 
            "$ref":"../../schema/wired/IpSlaType.json"
        }, 
        "probe_id":
        {
            "description": "Probe ID (Applicable for results)", 
            "type": "integer"
        }, 
        "probe_start_time":
        {
            "description": "First Probe Start Time (Applicable for Aggregate Results)", 
            "$ref":"../../schema/common/Timestamp.json"
        }, 
        "host_details":
        {
            "description": "Source and Destination Addresses/Ports details", 
            "$ref":"../../schema/wired/IpSlaUdpJitterHostDetails.json"
        }, 
        "packet_stats":
        {
            "description": "IP-SLA Jitter/ Jitter-For-Voip packet statistics", 
            "$ref":"../../schema/wired/IpSlaUdpPacketStats.json"
        }, 
        "extended_results":
        {
            "description": "IP-SLA Jitter/ Jitter-For-Voip extended results", 
            "$ref":"../../schema/wired/IpSlaExtendedResults.json"
        }, 
        "jitter_results":
        {
            "description": "IP-SLA UDP jitter results", 
            "$ref":"../../schema/wired/IpSlaUdpJitterResults.json"
        }, 
        "one_way_results":
        {
            "description": "IP-SLA One way results", 
            "$ref":"../../schema/wired/IpSlaOneWayResults.json"
        }, 
        "voice_scores":
        {
            "description": "IP-SLA Voice scores (Applicable for Jitter-For-Voip)", 
            "$ref":"../../schema/wired/IpSlaVoiceScores.json"
        }
    }
}
