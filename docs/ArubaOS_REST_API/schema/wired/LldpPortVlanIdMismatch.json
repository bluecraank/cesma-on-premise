{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Port Vlan Id mismatch Event", 
    "title": "LldpPortVlanIdMismatch", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port Number", 
            "type": "string", 
            "maxLength": 9
        }, 
        "local_pvid":
        {
            "description": "Local Pvid", 
            "type": "integer"
        }, 
        "remote_pvid":
        {
            "description": "Remote Pvid", 
            "type": "integer"
        }
    }
}
