{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Syslog ip address configuration data. Maximum 6 servers can be configured.", 
    "title": "SyslogServer", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "ip_address":
        {
            "description": "Syslog ip address.", 
            "$ref":"../../schema/common/IpAddress.json", 
            "sql.not_null": true
        }, 
        "control_description":
        {
            "description": "Syslog control_description. When not configured, this param is displayed as \"\".", 
            "type": "string", 
            "maxLength": 255, 
            "minLength": 1
        }, 
        "is_oobm":
        {
            "description": "Syslog oobm", 
            "type": "boolean"
        }, 
        "transport_protocol":
        {
            "description": "Syslog transport_protocol", 
            "$ref":"../../schema/wired/TransportProtocol.json", 
            "default_value": "TP_UDP"
        }, 
        "port":
        {
            "description": "Syslog port number. Default for UDP is 514, TCP is 1470 and TLS is 6514", 
            "minimum": 1024, 
            "maximum": 49151, 
            "type": "integer"
        }
    }
}
