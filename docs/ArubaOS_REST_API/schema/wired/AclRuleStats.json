{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "ACL Rule Status", 
    "title": "AclRuleStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "acl_id":
        {
            "description": "ACL Id", 
            "type": "string"
        }, 
        "sequence_no":
        {
            "description": "Sequence Number of each ACL Rule", 
            "type": "integer"
        }, 
        "hitcount":
        {
            "description": "Hardware Hitcounts for each ACL Rule", 
            "type": "integer"
        }
    }
}
