{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "VLAN IGMP port configuration.", 
    "title": "VlanIgmpPort", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "vlan_id":
        {
            "description": "The unique ID of the VLAN where IGMP is configured", 
            "minimum": 1, 
            "maximum": 4094, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "port_id":
        {
            "description": "Port ID", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "is_fastleave_enabled":
        {
            "description": "Whether IGMP fastleave is enabled", 
            "type": "boolean", 
            "default_value": true
        }, 
        "is_forcedfastleave_enabled":
        {
            "description": "Whether IGMP forced fastleave is enabled", 
            "type": "boolean"
        }, 
        "monitor_multicast_traffic":
        {
            "description": "IGMP monitor multicast traffic", 
            "$ref":"../../schema/wired/IgmpMulticastTraffic.json", 
            "default_value": "IMT_AUTO"
        }
    }
}
