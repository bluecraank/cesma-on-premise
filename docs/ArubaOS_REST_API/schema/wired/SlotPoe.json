{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Slot PoE configuration data.", 
    "title": "SlotPoe", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "slot_name":
        {
            "description": "Slot name.", 
            "type": "string", 
            "maxLength": 40, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "power_threshold_percentage":
        {
            "description": "Power threshold percentage", 
            "minimum": 1, 
            "maximum": 99, 
            "type": "integer", 
            "sql.not_null": true
        }
    }
}
