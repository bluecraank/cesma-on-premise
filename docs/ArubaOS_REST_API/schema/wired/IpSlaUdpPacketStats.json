{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "IP-SLA UDP Jitter/ Jitter-For-Voip Packet statistics", 
    "title": "IpSlaUdpPacketStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "packets_sent":
        {
            "description": "Number of packets sent", 
            "type": "integer"
        }, 
        "packets_received":
        {
            "description": "Number of packets received", 
            "type": "integer"
        }, 
        "min_rtt":
        {
            "description": "Minimum round trip time", 
            "type": "integer"
        }, 
        "max_rtt":
        {
            "description": "Maximum round trip time", 
            "type": "integer"
        }, 
        "avg_rtt":
        {
            "description": "Average round trip time", 
            "type": "integer"
        }, 
        "square_sum_rtt":
        {
            "description": "Square-Sum of round trip time", 
            "type": "integer"
        }, 
        "last_probe_time":
        {
            "description": "Last succeeded probe time", 
            "$ref":"../../schema/common/Timestamp.json"
        }
    }
}
