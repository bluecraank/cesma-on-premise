{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Display the results from transceiver cable diagnostic test.", 
    "title": "CableDiagnostics", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1
        }, 
        "cable_diagnostics_status":
        {
            "description": "Cable diagnostics status.", 
            "type": "array", 
            "minItems": 4, 
            "maxItems": 4, 
            "items":
            {
                "$ref":"../../schema/wired/CableDiagnosticsStatus.json"
            }
        }
    }
}
