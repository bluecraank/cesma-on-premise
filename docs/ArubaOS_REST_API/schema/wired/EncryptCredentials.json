{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Encrypt-credentials configuration data", 
    "title": "EncryptCredentials", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "encryption":
        {
            "description": "Gloable Encrypt-credentials status.", 
            "$ref":"../../schema/wired/EncryptCredentialsStatus.json", 
            "default_value": "ECS_DISABLED"
        }, 
        "pre_shared_key":
        {
            "description": "Pre-shared-key config", 
            "type": "string", 
            "default_value": "ECS_KEY_NONE"
        }, 
        "key_type":
        {
            "description": "Key type", 
            "$ref":"../../schema/wired/EncryptCredKeyType.json"
        }
    }
}
