{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Web Authentication Port configuration data", 
    "title": "WebAuthPort", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "static_url": ["/system/status/switch#/blades/data_ports/port_name"], 
            "dynamic_url": ["/system/status/switch#/blades/expansion_modules/data_ports/port_name"], 
            "sql.not_null": true
        }, 
        "is_webauth_enabled":
        {
            "description": "The status of Web authentication on the port", 
            "type": "boolean", 
            "sql.not_null": true
        }, 
        "unauthorized_vlan_id":
        {
            "description": "Unauthorized VLAN ID. Use 0 to reset Unauthorized VLAN ID.", 
            "minimum": 1, 
            "maximum": 4094, 
            "type": "integer", 
            "default_value": 0
        }, 
        "cached_reauth_period":
        {
            "description": "Time during which cached re-authentication is allowed on the port", 
            "minimum": 1, 
            "maximum": 2147483647, 
            "type": "integer", 
            "default_value": 0
        }, 
        "client_limit":
        {
            "description": "Maximum number of authenticated clients", 
            "minimum": 1, 
            "maximum": 256, 
            "type": "integer", 
            "default_value": 1
        }, 
        "client_moves":
        {
            "description": "Whether the client can move between ports", 
            "type": "boolean"
        }, 
        "logoff_period":
        {
            "description": "Inactivity time that the switch considers an implicit logoff", 
            "minimum": 1, 
            "maximum": 9999999, 
            "type": "integer", 
            "default_value": 300
        }, 
        "max_requests":
        {
            "description": "Maximum number of times the switch retransmits authentication requests", 
            "minimum": 1, 
            "maximum": 10, 
            "type": "integer", 
            "default_value": 3
        }, 
        "max_retries":
        {
            "description": "Maximum number of times a client can enter their credentials before authentication is considered to have failed", 
            "minimum": 1, 
            "maximum": 10, 
            "type": "integer", 
            "default_value": 3
        }, 
        "quiet_period":
        {
            "description": "Period of time the switch does not try to authenticate", 
            "minimum": 1, 
            "maximum": 65535, 
            "type": "integer", 
            "default_value": 60
        }, 
        "reauth_period":
        {
            "description": "Re-authentication timeout in seconds. Set to 0 to disable re-authentication", 
            "minimum": 0, 
            "maximum": 999999999, 
            "type": "integer", 
            "default_value": 0
        }, 
        "reauthenticate":
        {
            "description": "Force client re-authentication. Always return false", 
            "type": "boolean"
        }, 
        "redirect_url":
        {
            "description": "URL that the user should be redirected to after successful login", 
            "type": "string", 
            "maxLength": 127, 
            "minLength": 0
        }, 
        "server_timeout":
        {
            "description": "Authentication server response timeout", 
            "minimum": 1, 
            "maximum": 300, 
            "type": "integer", 
            "default_value": 300
        }, 
        "ssl_login":
        {
            "description": "Whether to enable SSL login", 
            "type": "boolean"
        }
    }
}
