{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Authentication Configuration for Login", 
    "title": "AuthenticationLoginConfig", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "primary_method":
        {
            "description": "Primary authentication method.", 
            "$ref":"../../schema/wired/PrimaryAuthMethod.json", 
            "default_value": "PAM_LOCAL", 
            "sql.not_null": true
        }, 
        "secondary_method":
        {
            "description": "Secondary authentication method.", 
            "$ref":"../../schema/wired/SecondaryAuthMethod.json", 
            "default_value": "SAM_NONE"
        }, 
        "server_group":
        {
            "description": "server group", 
            "type": "string", 
            "maxLength": 12, 
            "minLength": 0
        }
    }
}
