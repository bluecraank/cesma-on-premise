{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Subscription request for pushing events to remote server", 
    "title": "SubscriptionRequest", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "subscription_id":
        {
            "description": "Subscription ID", 
            "type": "integer", 
            "readonly": true, 
            "sql.auto_generated": true, 
            "sql.primary_key": true
        }, 
        "event_type":
        {
            "description": "Subscribe event type", 
            "$ref":"../../schema/wired/SubscribeEventType.json", 
            "sql.not_null": true
        }, 
        "client_url":
        {
            "description": "Client url to be subscribed for receiving events", 
            "type": "string", 
            "maxLength": 1024, 
            "sql.not_null": true
        }, 
        "system_module":
        {
            "description": "Subscribe for syslog message from specific system modules", 
            "type": "array", 
            "maxItems": 10, 
            "items":
            {
                "$ref": "../../schema/wired/LoggingSystemModuleType.json", 
                "default_value": "ALL_PASS"
            }
        }, 
        "severity_type":
        {
            "description": "Syslog Severity Type", 
            "$ref":"../../schema/wired/LoggingSeverityType.json", 
            "default_value": "DEBUG"
        }, 
        "event_filter":
        {
            "description": "Event id filter table", 
            "$ref":"../../schema/wired/EventIdFilter.json"
        }
    }
}
