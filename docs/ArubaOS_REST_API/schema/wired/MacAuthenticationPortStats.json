{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "MAC Authentication Port statistics data element.", 
    "title": "MacAuthenticationPortStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID.", 
            "type": "string", 
            "maxLength": 9
        }, 
        "port_state":
        {
            "description": "Port state.", 
            "$ref":"../../schema/wired/MacAuthenticationPortState.json"
        }, 
        "authenticated_client_mac_address":
        {
            "description": "MAC address of the connected client.", 
            "$ref":"../../schema/common/MacAddress.json"
        }, 
        "client_limit":
        {
            "description": "Client limit.", 
            "type": "integer"
        }, 
        "log_off_period_in_seconds":
        {
            "description": "Log off period.", 
            "type": "integer"
        }, 
        "reauthentication_period_in_seconds":
        {
            "description": "Reauthentication period.", 
            "type": "integer"
        }, 
        "authenticated_clients":
        {
            "description": "Authenticated clients.", 
            "type": "integer"
        }, 
        "unauthenticated_clients":
        {
            "description": "Unauthenticated clients.", 
            "type": "integer"
        }, 
        "guest_clients":
        {
            "description": "Guest clients.", 
            "type": "integer"
        }, 
        "authenticated_client_ip_address":
        {
            "description": "IP address of the connected port", 
            "$ref":"../../schema/common/IpAddress.json"
        }, 
        "username":
        {
            "description": "", 
            "type": "string"
        }, 
        "vlanid":
        {
            "description": "", 
            "type": "integer"
        }, 
        "inputOct":
        {
            "description": "", 
            "type": "integer"
        }, 
        "outputOct":
        {
            "description": "", 
            "type": "integer"
        }, 
        "inputPkt":
        {
            "description": "", 
            "type": "integer"
        }, 
        "outputPkt":
        {
            "description": "", 
            "type": "integer"
        }, 
        "authtype":
        {
            "description": "", 
            "$ref":"../../schema/wired/AuthType.json"
        }, 
        "conn_time":
        {
            "description": "", 
            "type": "integer"
        }
    }
