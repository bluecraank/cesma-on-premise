{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Structured syslog message data", 
    "title": "SyslogMessageData", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "event_id":
        {
            "description": "Event Id", 
            "$ref":"../../schema/wired/DeviceEventType.json"
        }, 
        "severity":
        {
            "description": "Severity", 
            "$ref":"../../schema/wired/LoggingSeverityType.json"
        }, 
        "system_module":
        {
            "description": "System Module", 
            "$ref":"../../schema/wired/LoggingSystemModuleType.json"
        }, 
        "date_time":
        {
            "description": "Timestamp", 
            "$ref":"../../schema/common/Timestamp.json"
        }, 
        "event_log_data":
        {
            "description": "Event specific data or information", 
            "type": "array", 
            "items":
            {
                "$ref":"../../schema/wired/EventDataType.json"
            }
        }
    }
}
