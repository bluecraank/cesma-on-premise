{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "BPS Member System Information", 
    "title": "BpsMemberSystemInfo", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "member_id":
        {
            "description": "Stack Member ID", 
            "type": "integer"
        }, 
        "rom_version":
        {
            "description": "ROM Version", 
            "type": "string"
        }, 
        "serial_number":
        {
            "description": "Serial Number", 
            "type": "string"
        }, 
        "uptime":
        {
            "description": "Uptime", 
            "$ref":"../../schema/wired/UpTime.json"
        }, 
        "cpu_util":
        {
            "description": "CPU Utilization (%)", 
            "type": "integer"
        }, 
        "total_memory":
        {
            "description": "", 
            "type": "integer"
        }, 
        "free_memory":
        {
            "description": "Free Memory in bytes", 
            "type": "integer"
        }, 
        "model":
        {
            "description": "BPS Member Product Model", 
            "type": "string"
        }, 
        "fan_info":
        {
            "description": "Fan Information", 
            "type": "array", 
            "items":
            {
                "$ref":"../../schema/wired/FanInfo.json"
            }
        }, 
        "temp_info":
        {
            "description": "Temperature Information", 
            "$ref":"../../schema/wired/TempInfo.json"
        }
    }
}
