{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Transceiver Cable Diagnostics", 
    "title": "TransceiverCableDiagnostics", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "cable_status":
        {
            "description": "Transceiver Cable Status.", 
            "$ref":"../../schema/wired/TransceiverCableStatus.json"
        }, 
        "mdi_pair":
        {
            "description": "Transceiver MDI Pair.", 
            "type": "string"
        }, 
        "distance_to_fault_in_meters":
        {
            "description": "Transceiver Distance to fault.", 
            "type": "integer"
        }, 
        "pair_skew_in_nanosecs":
        {
            "description": "Transceiver Pair Skew.", 
            "type": "integer"
        }, 
        "pair_polarity":
        {
            "description": "Transceiver Pair Polarity.", 
            "$ref":"../../schema/wired/TransceiverPairPolarity.json"
        }, 
        "mdi_mode":
        {
            "description": "Transceiver MDI Mode.", 
            "$ref":"../../schema/wired/TransceiverMdiMode.json"
        }
    }
}
