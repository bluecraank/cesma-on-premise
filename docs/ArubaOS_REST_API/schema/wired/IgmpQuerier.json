{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "VLAN IGMP Querier.", 
    "title": "IgmpQuerier", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "is_querier_enabled":
        {
            "description": "Whether IGMP Querier enabled on the VLAN", 
            "type": "boolean", 
            "default_value": true, 
            "sql.not_null": true
        }, 
        "interval":
        {
            "description": "IGMP querier interval. Value will be ignored if is_querier_enabled is false", 
            "minimum": 5, 
            "maximum": 300, 
            "type": "integer", 
            "default_value": 125
        }
    }
}
