{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Difference between running-config and config to be restored.", 
    "title": "CfgRestoreDiffStatus", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "diff_file_name":
        {
            "description": "config file name.", 
            "type": "string"
        }, 
        "diff_remove_list":
        {
            "description": "List of Commands in remove list.", 
            "type": "array", 
            "items":
            {
                "type": "string"
            }
        }, 
        "diff_add_list":
        {
            "description": "List of Commands in add list.", 
            "type": "array", 
            "items":
            {
                "type": "string"
            }
        }
    }
}
