{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Display the system variable details", 
    "title": "SystemVariable", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "system_variable_id":
        {
            "description": "System variable id. 1 is supported only in OOBM switches. 8 is supported only in standalone switches with modules. 9 is supported only in BPS enabled switches.", 
            "minimum": 1, 
            "maximum": 13, 
            "type": "integer"
        }, 
        "system_variable_name":
        {
            "description": "System variable name", 
            "type": "string", 
            "maxLength": 30
        }, 
        "system_variable_result":
        {
            "description": "System variable result. max_length can not be predicted as it varies with  configuration.", 
            "type": "string"
        }, 
        "system_variable_cert_digest":
        {
            "description": "Certificate and TA-Profile Digest details", 
            "$ref":"../../schema/wired/CertificateTaProfileDigestdetails.json", 
            "readonly": true, 
            "sql.auto_generated": true
        }
    }
}
