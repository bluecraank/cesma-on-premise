{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "VSF Port Configuration", 
    "title": "VsfPort", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "VSF Port ID", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "static_url": ["/system/status/switch#/blades/data_ports/port_name"], 
            "dynamic_url": ["/system/status/switch#/blades/expansion_modules/data_ports/port_name"], 
            "sql.not_null": true
        }, 
        "member_id":
        {
            "description": "VSF Member ID", 
            "minimum": 1, 
            "maximum": 8, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "link_id":
        {
            "description": "VSF Link ID", 
            "minimum": 1, 
            "maximum": 2, 
            "type": "integer", 
            "sql.not_null": true
        }
    }
}
