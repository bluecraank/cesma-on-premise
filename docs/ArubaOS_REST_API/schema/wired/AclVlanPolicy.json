{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "ACL VLAN Policy configuration data.", 
    "title": "AclVlanPolicy", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "vlan_id":
        {
            "description": "The unique ID of the VLAN.", 
            "minimum": 1, 
            "maximum": 4094, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "acl_id":
        {
            "description": "The ACL id used to reference it.", 
            "type": "string", 
            "maxLength": 90, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "direction":
        {
            "description": "Direction for which the rule is applied. Direction AD_CRF is applicable only for acl_type AT_CONNECTION_RATE_FILTER.", 
            "$ref":"../../schema/common/AclDirection.json", 
            "sql.not_null": true
        }
    }
}
