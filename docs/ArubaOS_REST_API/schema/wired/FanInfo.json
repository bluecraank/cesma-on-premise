        }, 
        "fan_failures":
        {
            "description": "Fan Failures", 
            "type": "integer", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "fan_location":
        {
            "description": "Fan Location", 
            "type": "string", 
            "readonly": true, 
            "sql.auto_generated": true
        }
    }
}
