{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "RadiusServerGroup.", 
    "title": "RadiusServerGroup", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "server_group_name":
        {
            "description": "Server Group Name.", 
            "type": "string", 
            "maxLength": 12, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "server_ip":
        {
            "description": "Radius server hosts", 
            "type": "array", 
            "minItems": 1, 
            "maxItems": 3, 
            "items":
            {
                "$ref":"../../schema/common/IpAddress.json"
            }, 
            "sql.not_null": true
        }
    }
}
