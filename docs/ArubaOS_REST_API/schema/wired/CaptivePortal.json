{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Captive Portal", 
    "title": "CaptivePortal", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "is_captive_portal_enabled":
        {
            "description": "Enable/Disable Captive Portal", 
            "type": "boolean"
        }, 
        "custom_profile":
        {
            "description": "Captive portal custom profile. Input null to remove custom_profile", 
            "$ref":"../../schema/wired/CaptivePortalProfile.json"
        }, 
        "predefined_profile":
        {
            "description": "Captive portal predefined profile", 
            "$ref":"../../schema/wired/CaptivePortalProfile.json", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "url_hash_key":
        {
            "description": "Hash key used to verify the integrity of the portal URL", 
            "type": "string", 
            "maxLength": 64, 
            "minLength": 0
        }
    }
}
