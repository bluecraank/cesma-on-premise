{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "VSF Member Information", 
    "title": "VsfMemberSystemInfo", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "model":
        {
            "description": "VSF Member Product Model", 
            "type": "string", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "rom_version":
        {
            "description": "ROM Version", 
            "type": "string", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "serial_num":
        {
            "description": "Serial Number", 
            "type": "string", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "uptime":
        {
            "description": "Uptime", 
            "$ref":"../../schema/wired/UpTime.json", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "cpu_util":
        {
            "description": "CPU Utilization (%)", 
            "type": "integer", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "member_id":
        {
            "description": "VSF Member ID", 
            "minimum": 1, 
            "maximum": 8, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "fan_info":
        {
            "description": "Fan Information", 
            "type": "array", 
            "items":
            {
                "$ref":"../../schema/wired/FanInfo.json"
            }, 
            "sql.auto_generated": true
        }, 
        "total_mem":
        {
            "description": "Total memory in bytes", 
            "type": "integer", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "free_mem":
        {
            "description": "Free memory in bytes", 
            "type": "integer", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "temp_info":
        {
            "description": "Temperature Information (only for 5400R)", 
            "$ref":"../../schema/wired/TempInfo.json", 
            "readonly": true, 
            "sql.auto_generated": true
        }
    }
}
