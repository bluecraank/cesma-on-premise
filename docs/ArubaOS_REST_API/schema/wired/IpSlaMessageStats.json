{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "IpSla Messages Status", 
    "title": "IpSlaMessageStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "ipsla_id":
        {
            "description": "IP-SLA ID.", 
            "type": "integer"
        }, 
        "ipsla_type":
        {
            "description": "IP-SLA Type.", 
            "$ref":"../../schema/wired/IpSlaType.json"
        }, 
        "status":
        {
            "description": "Status", 
            "$ref":"../../schema/wired/IpSlaMsgStatus.json"
        }, 
        "source_interface":
        {
            "description": "Source Interface(DHCP)", 
            "type": "integer"
        }, 
        "target_hostname":
        {
            "description": "Target Hostname(DNS)", 
            "type": "string"
        }, 
        "name_server_ip":
        {
            "description": "Name Server IP(DNS)", 
            "$ref":"../../schema/common/IpAddress.json"
        }, 
        "history_size":
        {
            "description": "History Size", 
            "type": "integer"
        }, 
        "last_probe":
        {
            "description": "Time of Last Received Probe", 
            "$ref":"../../schema/common/Timestamp.json"
        }, 
        "last_clear":
        {
            "description": "Time when Records were Last Cleared", 
            "$ref":"../../schema/common/Timestamp.json"
        }, 
        "threshold_hit_count":
        {
            "description": "Count of Threshold Hits", 
            "type": "integer"
        }, 
        "dns_messages":
        {
            "description": "Messages specific to DNS IP-SLA", 
            "$ref":"../../schema/wired/IpSlaDnsMessages.json"
        }, 
        "dhcp_messages":
        {
            "description": "Messages specific to DHCP IP-SLA", 
            "$ref":"../../schema/wired/IpSlaDhcpMessages.json"
        }
    }
}
