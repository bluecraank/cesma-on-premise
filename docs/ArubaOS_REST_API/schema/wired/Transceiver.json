{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Transceiver data.", 
    "title": "Transceiver", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Name of the Port.", 
            "type": "string", 
            "maxLength": 9
        }, 
        "type":
        {
            "description": "Type of Transceiver Port.", 
            "type": "string"
        }, 
        "product_number":
        {
            "description": "Transceiver Product Number.", 
            "type": "string"
        }, 
        "serial_number":
        {
            "description": "Transceiver Serial Number.", 
            "type": "string"
        }, 
        "part_number":
        {
            "description": "Transceiver Part Number.", 
            "type": "string"
        }, 
        "message":
        {
            "description": "Information message.", 
            "type": "string"
        }
    }
}
