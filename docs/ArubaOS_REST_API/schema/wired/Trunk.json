{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Static trunk configuration", 
    "title": "Trunk", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "static_url": ["/system/status/switch#/blades/data_ports/port_name"], 
            "dynamic_url": ["/system/status/switch#/blades/expansion_modules/data_ports/port_name"], 
            "sql.not_null": true
        }, 
        "trunk_group":
        {
            "description": "Port Trunk Group. Maximum trunk id is platform dependent.", 
            "type": "string", 
            "pattern": "^([tT][rR][kK]([1-9]|[1-9][0-9]|1[0-3][0-9]|14[0-4]))$", 
            "sql.not_null": true
        }
    }
}
