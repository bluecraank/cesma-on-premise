{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "BPS Member Configuration", 
    "title": "BpsMember", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "member_id":
        {
            "description": "Stack Member ID", 
            "minimum": 1, 
            "maximum": 10, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "type":
        {
            "description": "Family of the BPS member-switch being provisioned. Mandatory for creating a member and updating mac address. type cannot be changed  once it is provisioned.", 
            "$ref":"../../schema/wired/BpsJnumFamily.json"
        }, 
        "mac_address":
        {
            "description": "MAC Address of the BPS Member being provisioned. type is mandatory to configure mac_address.  MAC address cannot be modified after the member is joined.", 
            "$ref":"../../schema/common/MacAddress.json"
        }, 
        "model":
        {
            "description": "Model", 
            "type": "string", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "priority":
        {
            "description": "Priority", 
            "minimum": 1, 
            "maximum": 255, 
            "type": "integer", 
            "default_value": 128
        }, 
        "status":
        {
            "description": "Member Status", 
            "$ref":"../../schema/wired/StackMemberStatus.json", 
            "readonly": true, 
            "sql.auto_generated": true
        }
    }
}
