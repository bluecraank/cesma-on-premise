{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Dot1x authenticator statistics", 
    "title": "Dot1xAuthenticatorStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID", 
            "type": "string", 
            "maxLength": 9
        }, 
        "source_mac_address":
        {
            "description": "Source Mac address", 
            "type": "string"
        }, 
        "tx_req_id":
        {
            "description": "Tx ReqId", 
            "type": "integer"
        }, 
        "tx_req":
        {
            "description": "Tx Req", 
            "type": "integer"
        }, 
        "rx_start":
        {
            "description": "Rx start", 
            "type": "integer"
        }, 
        "rx_logoff":
        {
            "description": "Rx Logoff", 
            "type": "integer"
        }, 
        "rx_resp_id":
        {
            "description": "Rx RespId", 
            "type": "integer"
        }, 
        "rx_resp":
        {
            "description": "Rx Resp", 
            "type": "integer"
        }, 
        "rx_errors":
        {
            "description": "Rx Errors", 
            "type": "integer"
        }
    }
}
