{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Parity Error Recovery", 
    "title": "AsicParityError", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "module_name":
        {
            "description": "Module", 
            "type": "string"
        }, 
        "slot_id":
        {
            "description": "Slot Id", 
            "type": "string"
        }, 
        "memory_index":
        {
            "description": "Memory Index", 
            "type": "integer"
        }, 
        "memory_location":
        {
            "description": "Memory Location", 
            "type": "integer"
        }, 
        "parity_throttle_count":
        {
            "description": "Parity Throttle Count", 
            "type": "integer"
        }, 
        "mgr_parity_intervals":
        {
            "description": "Parity Intervals", 
            "type": "string"
        }
    }
}
