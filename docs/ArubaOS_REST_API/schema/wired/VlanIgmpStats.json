{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "VLAN IGMP statistics data.", 
    "title": "VlanIgmpStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "vlan_id":
        {
            "description": "The VLAN ID.", 
            "type": "integer"
        }, 
        "is_igmp_enabled":
        {
            "description": "Whether IGMP is enabled on the VLAN.", 
            "type": "boolean"
        }, 
        "querier_ip_address":
        {
            "description": "Querier IP Address", 
            "$ref":"../../schema/common/IpAddress.json"
        }, 
        "is_querier_elected_switch":
        {
            "description": "Is this switch is querier elected switch.", 
            "type": "boolean"
        }, 
        "filtered_groups_joined":
        {
            "description": "Filtered groups joined count.", 
            "type": "integer"
        }, 
        "standard_groups_joined":
        {
            "description": "Standard groups joined count.", 
            "type": "integer"
        }, 
        "static_groups_joined":
        {
            "description": "Static groups count.", 
            "type": "integer"
        }, 
        "total_groups_joined":
        {
            "description": "Total group joined count.", 
            "type": "integer"
        }, 
        "igmp_counters":
        {
            "description": "IGMP Counters.", 
            "$ref":"../../schema/wired/IgmpCounters.json"
        }, 
        "group":
        {
            "description": "IGMP Group list.", 
            "type": "array", 
            "items":
            {
                "$ref":"../../schema/wired/IgmpGroup.json"
            }
        }
    }
}
