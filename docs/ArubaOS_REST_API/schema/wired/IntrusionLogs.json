{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Intrusion Logs", 
    "title": "IntrusionLogs", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID", 
            "type": "string", 
            "maxLength": 9, 
            "sql.not_null": true
        }, 
        "mac_address":
        {
            "description": "Intruded mac address", 
            "type": "string", 
            "sql.not_null": true
        }, 
        "time_stamp":
        {
            "description": "Date and Time", 
            "$ref":"../../schema/common/Timestamp.json", 
            "sql.not_null": true
        }
    }
}
