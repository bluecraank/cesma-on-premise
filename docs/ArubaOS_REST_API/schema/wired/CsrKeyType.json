{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "", 
    "title": "CsrKeyType", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "ecdsa_curve_val":
        {
            "description": "ECSDA Curve value (256/384).", 
            "type": "integer", 
            "default_value": 256, 
            "enum": [256, 384]
        }, 
        "rsa_key_size":
        {
            "description": "RSA Key Size (1024/2048).", 
            "type": "integer", 
            "default_value": 1024, 
            "enum": [1024, 2048]
        }
    }
}
