{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "TACACS Profile", 
    "title": "TacacsProfile", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "dead_time":
        {
            "description": "Dead time for unavailable TACACS+ servers.", 
            "minimum": 0, 
            "maximum": 1440, 
            "type": "integer", 
            "default_value": 0
        }, 
        "time_out":
        {
            "description": "TACACS server response timeout.", 
            "minimum": 1, 
            "maximum": 255, 
            "type": "integer", 
            "default_value": 5
        }, 
        "global_auth_key":
        {
            "description": "Global Authentication Key for all TACACS servers", 
            "type": "string", 
            "maxLength": 100, 
            "minLength": 0
        }, 
        "source_ip_select":
        {
            "description": "TACACS Server source IP selection", 
            "type": "string", 
            "default_value": "Outgoing Interface"
        }, 
        "ordering_sequence":
        {
            "description": "TACACS Server Ordering Sequence", 
            "type": "boolean", 
            "default_value": false
        }
    }
}
