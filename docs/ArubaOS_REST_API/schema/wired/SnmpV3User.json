{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "SNMPv3 user.", 
    "title": "SnmpV3User", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "user_name":
        {
            "description": "The snmpv3 authentication username", 
            "type": "string", 
            "maxLength": 32, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "snmpv3_authentication_protocol":
        {
            "description": "The snmpv3 authentication protocol.", 
            "$ref":"../../schema/wired/Snmpv3AuthenticationProtocol.json", 
            "default_value": "SAP_MD5"
        }, 
        "authentication_password":
        {
            "description": "The snmpv3 authentication password", 
            "type": "string", 
            "maxLength": 32, 
            "minLength": 6
        }, 
        "snmpv3_authentication_privacy_protocol":
        {
            "description": "The snmpv3 authentication privacy protocol.", 
            "$ref":"../../schema/wired/Snmpv3AuthenticationPrivacyProtocol.json", 
            "default_value": "SAPP_DES"
        }, 
        "privacy_password":
        {
            "description": "The snmpv3 authentication privacy password", 
            "type": "string", 
            "maxLength": 32, 
            "minLength": 6
        }, 
        "snmpv3_v1_group":
        {
            "description": "The snmpv3 v1_group.", 
            "$ref":"../../schema/wired/Snmpv3GroupType.json"
        }, 
        "snmpv3_v2c_group":
        {
            "description": "The snmpv3 v2c_group.", 
            "$ref":"../../schema/wired/Snmpv3GroupType.json"
        }, 
        "snmpv3_v3_group":
        {
            "description": "The snmpv3 v3_group.", 
            "$ref":"../../schema/wired/Snmpv3GroupType.json"
        }
    }
}
