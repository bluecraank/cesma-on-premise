{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "IP-SLA configuration data", 
    "title": "IpSlaConfig", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "ipsla_id":
        {
            "description": "IP-SLA ID.", 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "ipsla_type":
        {
            "description": "IP-SLA Type.", 
            "$ref":"../../schema/wired/IpSlaType.json", 
            "sql.not_null": true
        }, 
        "ipsla_dns":
        {
            "description": "IP-SLA Configuration for type DNS (Mandatory only if ipsla_type is IST_DNS)", 
            "$ref":"../../schema/wired/IpSlaDns.json", 
            "sql.not_null": true
        }, 
        "ipsla_dhcp":
        {
            "description": "IP-SLA Configuration for type DHCP (Mandatory only if ipsla_type is IST_DHCP)", 
            "$ref":"../../schema/wired/IpSlaDhcp.json", 
            "sql.not_null": true
        }, 
        "ipsla_udp_jitter":
        {
            "description": "IP-SLA Configuration for type UDP Jitter (Mandatory only if ipsla_type is IST_UDP_JITTER)", 
            "$ref":"../../schema/wired/IpSlaUdpJitter.json"
        }, 
        "udp_jitter_for_voip":
        {
            "description": "IP-SLA Configuration for type UDP Jitter for VoIP (Mandatory only if ipsla_type is IST_UDP_JITTER_VOIP)", 
            "$ref":"../../schema/wired/IpSlaUdpJitterForVoip.json"
        }, 
        "is_ipsla_enabled":
        {
            "description": "IP-SLA Configuration enabled/disabled", 
            "type": "boolean"
        }, 
        "tos":
        {
            "description": "Type of Service value to be set in the test packet for the IP SLA", 
            "minimum": 0, 
            "maximum": 255, 
            "type": "integer"
        }, 
        "history_size":
        {
            "description": "The number of history records to be stored for the IP SLA.", 
            "minimum": 1, 
            "maximum": 50, 
            "type": "integer"
        }, 
        "schedule":
        {
            "description": "IP-SLA Schedule configuration", 
            "$ref":"../../schema/wired/IpSlaSchedule.json"
        }, 
        "packet_loss_monitor":
        {
            "description": "Configure Packet Loss Configuration", 
            "$ref":"../../schema/wired/IpSlaPacketLoss.json"
        }, 
        "test_commpletion_monitor":
        {
            "description": "Configure Test Completion Configuration", 
            "$ref":"../../schema/wired/IpSlaTestCompletion.json"
        }, 
        "rtt_monitor":
        {
            "description": "Configure RTT Threshold Configuration", 
            "$ref":"../../schema/wired/IpSlaRtt.json"
        }, 
        "src_dst_jitter_monitor":
        {
            "description": "Configure Source to Destination Jitter Monitor", 
            "$ref":"../../schema/wired/IpSlaJitterMonitor.json"
        }, 
        "dst_src_jitter_monitor":
        {
            "description": "Configure Destination to Source Jitter Monitor", 
            "$ref":"../../schema/wired/IpSlaJitterMonitor.json"
        }, 
        "src_dst_time_monitor":
        {
            "description": "Configure Source to Destination Time Monitor", 
            "$ref":"../../schema/wired/IpSlaJitterMonitor.json"
        }, 
        "dst_src_time_monitor":
        {
            "description": "Configure Destination to Source Time Monitor", 
            "$ref":"../../schema/wired/IpSlaJitterMonitor.json"
        }, 
        "mos_jitter_monitor":
        {
            "description": "Configure MOS Jitter Monitor", 
            "$ref":"../../schema/wired/IpSlaJitterMonitor.json"
        }, 
        "icpif_jitter_monitor":
        {
            "description": "Configure ICPIF Jitter Monitor", 
            "$ref":"../../schema/wired/IpSlaJitterMonitor.json"
        }
    }
}
