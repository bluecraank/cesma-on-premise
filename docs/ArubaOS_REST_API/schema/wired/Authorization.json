{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Authorization", 
    "title": "Authorization", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "authorization_method":
        {
            "description": "Command Authorization method", 
            "$ref":"../../schema/wired/AuthorizationMethod.json", 
            "default_value": "AZM_NONE"
        }, 
        "access_level":
        {
            "description": "Command access-level", 
            "$ref":"../../schema/wired/AuthorizationAcessLevel.json", 
            "default_value": "AZL_ALL"
        }, 
        "autz_uri_method":
        {
            "description": "REST-URI Authorization method", 
            "$ref":"../../schema/wired/AuthorizationMethod.json", 
            "default_value": "AZM_NONE"
        }, 
        "autz_uri_level":
        {
            "description": "REST_URI access-level", 
            "$ref":"../../schema/wired/AuthorizationAcessLevel.json", 
            "default_value": "AZL_ALL"
        }
    }
}
