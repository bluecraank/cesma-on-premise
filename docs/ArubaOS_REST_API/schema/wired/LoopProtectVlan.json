{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Loop protection on the specified vlan", 
    "title": "LoopProtectVlan", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "vlan_id":
        {
            "description": "Vlan id.", 
            "minimum": 1, 
            "maximum": 4094, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "is_vlan_loop_protected":
        {
            "description": "Enable/Disable loop protection on the specified vlan.", 
            "type": "boolean", 
            "sql.not_null": true
        }
    }
}
