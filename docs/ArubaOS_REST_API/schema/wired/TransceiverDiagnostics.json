{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Transceiver Diagnostics data.", 
    "title": "TransceiverDiagnostics", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Name of the Port.", 
            "type": "string", 
            "maxLength": 9
        }, 
        "type":
        {
            "description": "Type of Transceiver Port.", 
            "type": "string"
        }, 
        "model":
        {
            "description": "Transceiver Model.", 
            "type": "string"
        }, 
        "connector_type":
        {
            "description": "Transceiver Connector Type.", 
            "type": "string"
        }, 
        "wave_length":
        {
            "description": "Transceiver Wavelength.", 
            "type": "string"
        }, 
        "transfer_distance":
        {
            "description": "Transfer distance.", 
            "type": "string"
        }, 
        "diagnostic_support_type":
        {
            "description": "Transceiver Diagnostics Support Type.", 
            "$ref":"../../schema/wired/TransceiverDiagnosticSupport.json"
        }, 
        "serial_number":
        {
            "description": "Transceiver Serial Number.", 
            "type": "string"
        }, 
        "dom_diagnostics":
        {
            "description": "DOM Diagnostics.", 
            "$ref":"../../schema/wired/TransceiverDomDiagnostics.json"
        }, 
        "vct_diagnostics":
        {
            "description": "VCT Diagnostics.", 
            "$ref":"../../schema/wired/TransceiverVctDiagnostics.json"
        }
    }
}
