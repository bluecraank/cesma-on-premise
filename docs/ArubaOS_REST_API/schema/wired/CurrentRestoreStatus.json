{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Display the current status of cfg-restore.", 
    "title": "CurrentRestoreStatus", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "status":
        {
            "description": "Current Status of the restore performed.", 
            "$ref":"../../schema/wired/CfgRestoreStatus.json"
        }, 
        "failure_reason":
        {
            "description": "Failure reason", 
            "type": "string", 
            "maxLength": 688
        }
    }
}
