{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Dot1x Device Identity", 
    "title": "Dot1xDeviceIdentity", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "device_name":
        {
            "description": "Device Identity name.", 
            "type": "string", 
            "maxLength": 20, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "port_id":
        {
            "description": "Port ID", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "sql.not_null": true
        }
    }
}
