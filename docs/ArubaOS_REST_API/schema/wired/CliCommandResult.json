{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "CLI command result", 
    "title": "CliCommandResult", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "cmd":
        {
            "description": "CLI command executed.", 
            "type": "string", 
            "sql.not_null": true
        }, 
        "result_base64_encoded":
        {
            "description": "Base64 encoded result", 
            "type": "string", 
            "sql.not_null": true
        }, 
        "error_msg":
        {
            "description": "Error message.", 
            "type": "string", 
            "sql.not_null": true
        }, 
        "status":
        {
            "description": "CLI command execution status", 
            "$ref":"../../schema/wired/CliCmdStatus.json", 
            "sql.not_null": true
        }
    }
}
