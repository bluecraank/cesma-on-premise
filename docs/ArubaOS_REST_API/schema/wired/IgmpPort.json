{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "IGMP port fastlearn.", 
    "title": "IgmpPort", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "is_fastlearn_enabled":
        {
            "description": "Whether IGMP fastlearn is enabled", 
            "type": "boolean", 
            "sql.not_null": true
        }
    }
}
