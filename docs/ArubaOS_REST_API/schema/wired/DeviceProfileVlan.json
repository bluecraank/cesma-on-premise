{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Configuration for device-profile association of tagged and untagged vlans", 
    "title": "DeviceProfileVlan", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "device_profile_name":
        {
            "description": "device-profile Name", 
            "type": "string", 
            "maxLength": 32, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "tagged_vlan_id":
        {
            "description": "Tagged VLAN-ID", 
            "type": "integer", 
            "sql.not_null": true
        }
    }
}
