{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "SNMP Server Trap Settings", 
    "title": "SnmpServerTraps", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "arp_protect":
        {
            "description": "Traps for Dynamic ARP Protection.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "auth_server_fail":
        {
            "description": "Traps reporting authentication server unreachable.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "dhcp_server":
        {
            "description": "Traps for DHCP-Server.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "dhcp_snooping":
        {
            "description": "Traps for DHCPv6 snooping out of resource.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "dhcpv6_snooping_out_of_resource":
        {
            "description": "Traps for DHCPv6 snooping out of resource.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "dhcpv6_snooping_errant_replies":
        {
            "description": "Traps for DHCPv6 snooping errant replies.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "dyn_ip_lockdown":
        {
            "description": "Traps for Dynamic Ip Lockdown.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "dyn_ipv6_ld_out_of_resources":
        {
            "description": "Enable traps for Dynamic IPv6 Lockdown out of resources.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "dyn_ipv6_ld_violations":
        {
            "description": "Enable traps for Dynamic IPv6 Lockdown violations.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "login_failure_mgr":
        {
            "description": "Traps for management interface login failure.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "mac_count_notify":
        {
            "description": "Traps for MAC addresses learned on the specified ports exceeds the threshold.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "nd_snooping_out_of_resources":
        {
            "description": "The trap for nd snooping out of resources.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "password_change_mgr":
        {
            "description": "Traps for management interface password change.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "port_security":
        {
            "description": "Traps for port access authentication failure.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }, 
        "startup_config_change":
        {
            "description": "Traps for changes to the startup config.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_DISABLE"
        }, 
        "mac_notify":
        {
            "description": "Traps for notifying mac related settings.", 
            "$ref":"../../schema/wired/SnmpServerMacNotifyTrap.json"
        }, 
        "running_config_changes":
        {
            "description": "Traps settings for running config change.", 
            "$ref":"../../schema/wired/SnmpServerRunConfTrap.json"
        }, 
        "snmp_authentication":
        {
            "description": "Select RFC1157 (standard) or HP-ICF-SNMP (extended) traps.", 
            "$ref":"../../schema/wired/SnmpAuthTrapMode.json", 
            "default_value": "SATM_EXTENDED"
        }, 
        "macsec_failure":
        {
            "description": "Enable the MACsec Connectivity Association (CA) failure trap.", 
            "$ref":"../../schema/wired/SnmpTrapMode.json", 
            "default_value": "STM_ENABLE"
        }
    }
}
