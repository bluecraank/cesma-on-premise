{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Telnet server configuration", 
    "title": "TelnetServer", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "is_telnet_server_enabled":
        {
            "description": "Whether telnet server is enabled or disabled", 
            "type": "boolean", 
            "default_value": true, 
            "sql.not_null": true
        }
    }
}
