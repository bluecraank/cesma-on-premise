{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Subscription status", 
    "title": "SubscriptionStatus", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "subscription_id":
        {
            "description": "Subscription ID", 
            "type": "integer", 
            "readonly": true, 
            "sql.auto_generated": true, 
            "sql.primary_key": true
        }, 
        "event_type":
        {
            "description": "Subscribed event type", 
            "$ref":"../../schema/wired/SubscribeEventType.json", 
            "sql.not_null": true
        }, 
        "client_url":
        {
            "description": "Client url [subscribed/ not subscribed] to receive events", 
            "type": "string", 
            "sql.not_null": true
        }, 
        "system_module":
        {
            "description": "Events specific to system module", 
            "type": "array", 
            "maxItems": 10, 
            "items":
            {
                "$ref": "../../schema/wired/LoggingSystemModuleType.json", 
                "default_value": "ALL_PASS"
            }
        }, 
        "severity_type":
        {
            "description": "Syslog Severity Type", 
            "$ref":"../../schema/wired/LoggingSeverityType.json", 
            "default_value": "DEBUG"
        }, 
        "event_filter":
        {
            "description": "Event id filter table", 
            "$ref":"../../schema/wired/EventIdFilter.json"
        }
    }
}
