{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "802.1X Authentication method data.", 
    "title": "Dot1xAuthenticationMethod", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "primary_authentication_method":
        {
            "description": "Primary Authentication Method.", 
            "$ref":"../../schema/wired/Dot1xPrimaryAuthenticationMethod.json", 
            "default_value": "DPAM_LOCAL", 
            "sql.not_null": true
        }, 
        "server_group":
        {
            "description": "Server Group", 
            "type": "string", 
            "maxLength": 12, 
            "minLength": 0
        }, 
        "secondary_authentication_method":
        {
            "description": "Secondary Authentication Method.", 
            "$ref":"../../schema/wired/Dot1xSecondaryAuthenticationMethod.json", 
            "default_value": "DSAM_NONE"
        }
    }
}
