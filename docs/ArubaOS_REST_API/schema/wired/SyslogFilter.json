{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Syslog Filter configurations.", 
    "title": "SyslogFilter", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "filter_name":
        {
            "description": "filter name", 
            "type": "string", 
            "maxLength": 16, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "is_enabled":
        {
            "description": "is_filter_enabled", 
            "type": "boolean", 
            "sql.not_null": true
        }, 
        "default_action":
        {
            "description": "default action.", 
            "$ref":"../../schema/wired/DefaultAction.json", 
            "default_value": "PERMIT", 
            "sql.not_null": true
        }
    }
}
