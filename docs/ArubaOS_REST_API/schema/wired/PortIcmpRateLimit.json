{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "ICMP Rate limit configuration for port traffic", 
    "title": "PortIcmpRateLimit", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "icmp_traffic_type":
        {
            "description": "Icmp Traffic type", 
            "$ref":"../../schema/wired/PortIcmpTrafficType.json", 
            "default_value": "PITT_IP_V4"
        }, 
        "icmp_rate_limit":
        {
            "description": "Icmp Rate limit value", 
            "$ref":"../../schema/wired/PortTrafficRateLimit.json", 
            "sql.not_null": true
        }
    }
}
