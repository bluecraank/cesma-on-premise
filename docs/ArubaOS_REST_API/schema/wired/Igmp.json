{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "IGMP configuration data.", 
    "title": "Igmp", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "delayed_flush":
        {
            "description": "IGMP delayed flush", 
            "minimum": 0, 
            "maximum": 255, 
            "type": "integer", 
            "default_value": 0
        }, 
        "filter_unknown_mcast":
        {
            "description": "Whether IGMP filter-unknown-mcast is enabled", 
            "type": "boolean", 
            "default_value": false
        }, 
        "lookup_mode":
        {
            "description": "IGMP lookup mode", 
            "$ref":"../../schema/wired/IgmpLookupMode.json", 
            "default_value": "ILM_MAC"
        }
    }
}
