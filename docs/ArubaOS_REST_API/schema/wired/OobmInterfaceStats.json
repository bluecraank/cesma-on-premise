{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "OOBM Interface statistics data.", 
    "title": "OobmInterfaceStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_type":
        {
            "description": "OOBM Port type.", 
            "$ref":"../../schema/wired/InterfaceType.json"
        }, 
        "operational_status":
        {
            "description": "OOBM Port Operational Status.", 
            "$ref":"../../schema/common/InterfaceOperStatus.json"
        }, 
        "member_id":
        {
            "description": "Member Id for stacked switches. Return null for non-stacked switches", 
            "type": "integer"
        }
    }
}
