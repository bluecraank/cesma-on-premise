{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "QoS Trust configurations data.", 
    "title": "QosTrust", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "port_id", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "trust_mode":
        {
            "description": "QoS Trust Mode Configuration for the port.", 
            "$ref":"../../schema/wired/QosTrustMode.json", 
            "default_value": "QTM_DEFAULT"
        }, 
        "trusted_device":
        {
            "description": "QoS Trusted Devices.  trusted_device is applicable and mandatory only for trust_mode QTM_DEVICE.", 
            "$ref":"../../schema/wired/QosTrustDevice.json"
        }, 
        "trusted_device_state":
        {
            "description": "QoS Trusted Device's state.  trusted_device_state is applicable only for trust_mode QTM_DEVICE.", 
            "$ref":"../../schema/wired/QosTrustDevice.json", 
            "readonly": true, 
            "sql.auto_generated": true
        }
    }
}
