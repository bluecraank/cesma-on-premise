{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "IpSla History", 
    "title": "IpSlaHistory", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "ipsla_id":
        {
            "description": "IP-SLA ID.", 
            "type": "integer"
        }, 
        "ipsla_type":
        {
            "description": "IP-SLA Type.", 
            "$ref":"../../schema/wired/IpSlaType.json"
        }, 
        "minimum_rtt":
        {
            "description": "Minimum RTT", 
            "type": "integer"
        }, 
        "maximum_rtt":
        {
            "description": "Maximum RTT", 
            "type": "integer"
        }, 
        "average_rtt":
        {
            "description": "Average RTT", 
            "type": "integer"
        }, 
        "total_rtt":
        {
            "description": "Total RTT", 
            "type": "integer"
        }, 
        "rtt_2":
        {
            "description": "RTT2 - sum of RTT squared", 
            "type": "integer"
        }, 
        "ip_sla_history_data":
        {
            "description": "IP-SLA history data", 
            "type": "array", 
            "items":
            {
                "$ref":"../../schema/wired/IpSlaHistoryData.json"
            }
        }
    }
}
