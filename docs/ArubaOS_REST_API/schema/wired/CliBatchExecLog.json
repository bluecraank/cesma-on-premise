{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "CLI Batch Execution Log", 
    "title": "CliBatchExecLog", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "cmd":
        {
            "description": "Command executed", 
            "type": "string", 
            "sql.not_null": true
        }, 
        "status":
        {
            "description": "Command status", 
            "$ref":"../../schema/wired/CliCmdStatus.json", 
            "sql.not_null": true
        }, 
        "result":
        {
            "description": "Result string", 
            "type": "string", 
            "sql.not_null": true
        }
    }
}
