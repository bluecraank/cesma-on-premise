{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "SNMP Server Community Settings", 
    "title": "SnmpServerCommunity", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "access_type":
        {
            "description": "Level of access for community (operator/manager).", 
            "$ref":"../../schema/common/UserType.json", 
            "default_value": "UT_OPERATOR"
        }, 
        "community_name":
        {
            "description": "Name of the SNMP community.", 
            "type": "string", 
            "maxLength": 32, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "restricted":
        {
            "description": "Type of access for community (restricted/unrestricted).", 
            "type": "boolean", 
            "default_value": true
        }
    }
}
