{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "IpSla DnsMessages", 
    "title": "IpSlaDnsMessages", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "successful_dns_resolutions":
        {
            "description": "Number of Successful DNS Resolutions", 
            "type": "integer"
        }, 
        "resolution_timed_out":
        {
            "description": "Resolution Timed Out", 
            "type": "integer"
        }, 
        "local_interface_is_down":
        {
            "description": "Local Interface is Down", 
            "type": "integer"
        }, 
        "internal_error":
