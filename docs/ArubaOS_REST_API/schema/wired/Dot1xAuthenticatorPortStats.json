{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "802.1X Authenticator Port statistics data.", 
    "title": "Dot1xAuthenticatorPortStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Physical port ID", 
            "type": "string", 
            "maxLength": 9
        }, 
        "is_authenticator_enabled":
        {
            "description": "Port authentication status : enable/disable", 
            "type": "boolean"
        }, 
        "state":
        {
            "description": "Authenticator port connection status", 
            "$ref":"../../schema/wired/Dot1xAuthenticatorPortState.json"
        }, 
        "authenticated_client_mac_address":
        {
            "description": "Mac address of the connected client", 
            "$ref":"../../schema/common/MacAddress.json"
        }, 
        "authenticated_client_ip_address":
        {
            "description": "IP address of the connected port", 
            "$ref":"../../schema/common/IpAddress.json"
        }, 
        "authentication_frames_received":
        {
            "description": "Authentication EAPOL frame received", 
            "type": "integer"
        }, 
        "authentication_request_id_frames_received":
        {
            "description": "Authentication request id frames", 
            "type": "integer"
        }, 
        "authentication_start_frames_received":
        {
            "description": "Authentication start frame received", 
            "type": "integer"
        }, 
        "authentication_logoff_frames_received":
        {
            "description": "Log off frame received", 
            "type": "integer"
        }, 
        "authentication_response_id_frames_received":
        {
            "description": "Authentication response id frames", 
            "type": "integer"
        }, 
        "authentication_response_frames_received":
        {
            "description": "Authentication response frame received", 
            "type": "integer"
        }, 
        "authentication_error_frames_received":
        {
            "description": "Wrong or error EAPOL frames send", 
            "type": "integer"
        }, 
        "authentication_session_frames_received":
        {
            "description": "Session frame received", 
            "type": "integer"
        }, 
        "authentication_session_frames_sent":
        {
            "description": "Session frame sent", 
            "type": "integer"
        }, 
        "username":
        {
            "description": "", 
            "type": "string"
        }, 
        "vlanid":
        {
            "description": "", 
            "type": "integer"
        }, 
        "inputOct":
        {
            "description": "", 
            "type": "integer"
        }, 
        "outputOct":
        {
            "description": "", 
            "type": "integer"
        }, 
        "inputPkt":
        {
            "description": "", 
            "type": "integer"
        }, 
        "outputPkt":
        {
            "description": "", 
            "type": "integer"
        }, 
        "authtype":
        {
            "description": "", 
            "$ref":"../../schema/wired/AuthType.json"
        }, 
        "conn_time":
        {
            "description": "", 
            "type": "integer"
        }
    }
}
