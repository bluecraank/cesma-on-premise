{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Include-Credentials configuration data. This option allows passwords/credentials to be sent in response through REST. By default passwords/credentials are NOT sent in response through REST.", 
    "title": "IncludeCredentials", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "include_credentials_in_response":
        {
            "description": "Global Include-Credentials Status.", 
            "$ref":"../../schema/wired/IncludeCredentialsStatus.json", 
            "default_value": "ICS_DISABLED"
        }, 
        "store_in_config":
        {
            "description": "Store-in-config status", 
            "type": "boolean", 
            "default_value": false
        }, 
        "password_config":
        {
            "description": "", 
            "type": "boolean", 
            "default_value": false
        }, 
        "user_type":
        {
            "description": "Whether manager or operator", 
            "$ref":"../../schema/common/UserType.json", 
            "default_value": "UT_OPERATOR"
        }, 
        "username":
        {
            "description": "username", 
            "type": "string", 
            "maxLength": 255, 
            "minLength": 1
        }, 
        "password":
        {
            "description": "Password", 
            "type": "string", 
            "maxLength": 64, 
            "minLength": 8
        }
    }
}
