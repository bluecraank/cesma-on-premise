{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Transceiver Event Message.", 
    "title": "TransceiverEvent", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port Number in which transceiver is connected.", 
            "type": "string", 
            "maxLength": 9
        }, 
        "product_number":
        {
            "description": "Transceiver Product Number.", 
            "type": "string"
        }, 
        "restriction_limit":
        {
            "description": "Transceiver Restriction Limit.", 
            "type": "integer"
        }, 
        "self_test_error_number":
        {
            "description": "Transceiver Self Test error number.", 
            "type": "integer"
        }
    }
}
