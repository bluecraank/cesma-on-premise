{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Stacking BPS Enable", 
    "title": "BpsEnableCommand", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "member_id":
        {
            "description": "Specify the preferred member ID for this switch", 
            "minimum": 1, 
            "maximum": 10, 
            "type": "integer"
        }, 
        "priority":
        {
            "description": "Specify the priority of this switch", 
            "minimum": 1, 
            "maximum": 255, 
            "type": "integer", 
            "default_value": 128
        }
    }
}
