{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Installed Modules Information", 
    "title": "ModuleInfo", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "slot":
        {
            "description": "Slot", 
            "type": "string", 
            "readonly": true, 
            "sql.not_null": true, 
            "sql.primary_key": true
        }, 
        "description":
        {
            "description": "Module Description", 
            "type": "string", 
            "sql.not_null": true
        }, 
        "serial_number":
        {
            "description": "Serial Number", 
            "type": "string"
        }, 
        "module_status":
        {
            "description": "Module Status", 
            "$ref":"../../schema/wired/ModuleStatus.json"
        }, 
        "is_core_dump_present":
        {
            "description": "Core Dump status", 
            "type": "boolean"
        }, 
        "version":
        {
            "description": "Module Version", 
            "type": "integer"
        }, 
        "member_id":
        {
            "description": "Member id . Applicable for BPS enabled switches", 
            "type": "integer"
        }
    }
}
