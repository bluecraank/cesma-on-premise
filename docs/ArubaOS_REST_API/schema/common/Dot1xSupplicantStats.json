{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Dot1x supplicant statistics", 
    "title": "Dot1xSupplicantStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "port id", 
            "type": "string", 
            "maxLength": 9
        }, 
        "source_mac_address":
        {
            "description": "Source Mac address", 
            "type": "string", 
            "maxLength": 23
        }, 
        "tx_start":
        {
            "description": "Tx start", 
            "type": "integer"
        }, 
        "tx_logoff":
        {
            "description": "Tx Logoff", 
            "type": "integer"
        }, 
        "tx_resp_id":
        {
            "description": "Tx RespId", 
            "type": "integer"
        }, 
        "tx_resp":
        {
            "description": "Tx Resp", 
            "type": "integer"
        }, 
        "rx_req_id":
        {
            "description": "Rx ReqId", 
            "type": "integer"
        }, 
        "rx_req":
        {
            "description": "Rx Req", 
            "type": "integer"
        }, 
        "rx_errors":
        {
            "description": "Rx Errors", 
            "type": "integer"
        }
    }
}
