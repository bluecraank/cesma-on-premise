{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "802.1X Supplicant Port configuration data.", 
    "title": "Dot1xSupplicantPort", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_id":
        {
            "description": "Port ID.", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "static_url": ["/system/status/switch#/blades/data_ports/port_name"], 
            "dynamic_url": ["/system/status/switch#/blades/expansion_modules/data_ports/port_name"], 
            "sql.not_null": true
        }, 
        "is_supplicant_enabled":
        {
            "description": "802.1X Supplicant Port admin status.", 
            "type": "boolean", 
            "sql.not_null": true
        }, 
        "username":
        {
            "description": "The authentication username", 
            "type": "string", 
            "maxLength": 64, 
            "minLength": 1
        }, 
        "password":
        {
            "description": "The authentication identity password", 
            "type": "string", 
            "maxLength": 64, 
            "minLength": 1
        }, 
        "supplicant_state":
        {
            "description": "Supplicant state", 
            "type": "string", 
            "maxLength": 14, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "auth_timeout":
        {
            "description": "Auth timeout.", 
            "minimum": 1, 
            "maximum": 300, 
            "type": "integer", 
            "default_value": 30
        }, 
        "held_period":
        {
            "description": "Held period.", 
            "minimum": 0, 
            "maximum": 65535, 
            "type": "integer", 
            "default_value": 60
        }, 
        "start_period":
        {
            "description": "Start period.", 
            "minimum": 1, 
            "maximum": 300, 
            "type": "integer", 
            "default_value": 30
        }, 
        "max_start":
        {
            "description": "Max start.", 
            "minimum": 1, 
            "maximum": 10, 
            "type": "integer", 
            "default_value": 3
        }
    }
}
