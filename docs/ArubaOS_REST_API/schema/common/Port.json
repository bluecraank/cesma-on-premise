{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Port configuration data.", 
    "title": "Port", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "id":
        {
            "description": "The unique ID of the Port.", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "name":
        {
            "description": "The name of the Port. An \"\" denotes removal of already configured name if exists.", 
            "type": "string", 
            "maxLength": 64, 
            "minLength": 0, 
            "sql.unique": true
        }, 
        "is_port_enabled":
        {
            "description": "Whether the Port is enabled.", 
            "type": "boolean", 
            "default_value": true
        }, 
        "is_port_up":
        {
            "description": "Specifies Whether the  Link status is up or down.", 
            "type": "boolean", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "config_mode":
        {
            "description": "The Port Config Mode.", 
            "$ref":"../../schema/common/PortConfigMode.json", 
            "default_value": "PCM_AUTO"
        }, 
        "trunk_mode":
        {
            "description": "The Port Trunk Mode.", 
            "$ref":"../../schema/common/PortTrunkType.json", 
            "default_value": "PTT_NONE", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "lacp_status":
        {
            "description": "The Ports LACP Status.", 
            "$ref":"../../schema/common/LacpStatus.json", 
            "default_value": "LAS_DISABLED"
        }, 
        "trunk_group":
        {
            "description": "The trunk group associated with this Port.", 
            "type": "string", 
            "maxLength": 6, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "is_flow_control_enabled":
        {
            "description": "Whether the Port's flow control is enabled.", 
            "type": "boolean", 
            "default_value": false
        }, 
        "is_dsnoop_port_trusted":
        {
            "description": "Specifies if the port is trusted. @wired only", 
            "type": "boolean", 
            "default_value": false
        }
    }
}
