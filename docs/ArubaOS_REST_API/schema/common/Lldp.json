{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Global LLDP configuration", 
    "title": "Lldp", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "admin_status":
        {
            "description": "LLDP Global status", 
            "$ref":"../../schema/common/LldpAdminStatus.json", 
            "default_value": "LLAS_ENABLED"
        }, 
        "transmit_interval":
        {
            "description": "LLDP Transmit Interval (Refresh interval). Should be greater than or equal to 4 * reinit_interval.", 
            "minimum": 5, 
            "maximum": 32768, 
            "type": "integer", 
            "default_value": 30
        }, 
        "hold_time_multiplier":
        {
            "description": "LLDP Hold time Multiplier", 
            "minimum": 2, 
            "maximum": 10, 
            "type": "integer", 
            "default_value": 4
        }, 
        "fast_start_count":
        {
            "description": "LLDP Fast Start Count", 
            "minimum": 1, 
            "maximum": 10, 
            "type": "integer", 
            "default_value": 5
        }, 
        "reinit_interval":
        {
            "description": "LLDP Reinit Interval (Delay Interval)", 
            "type": "integer", 
            "default_value": 2, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "notification_interval":
        {
            "description": "LLDP Notification Interval", 
            "type": "integer", 
            "default_value": 5, 
            "readonly": true, 
            "sql.auto_generated": true
        }
    }
}
