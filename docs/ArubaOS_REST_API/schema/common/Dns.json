{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "DNS configuration data.", 
    "title": "Dns", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "dns_config_mode":
        {
            "description": "DNS configuration Mode.", 
            "$ref":"../../schema/common/DnsConfigMode.json", 
            "default_value": "DCM_DHCP", 
            "sql.not_null": true
        }, 
        "server_1":
        {
            "description": "The first manually configured DNS server.", 
            "$ref":"../../schema/common/IpAddress.json"
        }, 
        "server_2":
        {
            "description": "The second manually configured DNS server.", 
            "$ref":"../../schema/common/IpAddress.json"
        }, 
        "dns_domain_names":
        {
            "description": "Array of Manually configured DNS server domain names.", 
            "type": "array", 
            "minItems": 0, 
            "maxItems": 5, 
            "items":
            {
                "type": "string", 
                "maxLength": 255, 
                "minLength": 1
            }
        }, 
        "server_3":
        {
            "description": "The third manually configured DNS server.", 
            "$ref":"../../schema/common/IpAddress.json"
        }, 
        "server_4":
        {
            "description": "The fourth manually configured DNS server.", 
            "$ref":"../../schema/common/IpAddress.json"
        }
    }
}
