{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "VLAN Port configuration data.", 
    "title": "VlanPort", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "vlan_id":
        {
            "description": "The unique ID of the VLAN.", 
            "minimum": 1, 
            "maximum": 4094, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "port_id":
        {
            "description": "The unique ID of the Port.", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "static_url": ["/system/status/switch#/blades/data_ports/port_name", 
            "/lacp/port/#/lacp_element/trunk_group", 
            "/trunk/port/#/trunk_element/trunk_group"], 
            "dynamic_url": ["/system/status/switch#/blades/expansion_modules/data_ports/port_name"], 
            "sql.not_null": true
        }, 
        "port_mode":
        {
            "description": "The Ports mode.", 
            "$ref":"../../schema/common/PortMode.json", 
            "sql.not_null": true
        }
    }
}
