{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Per port LLDP configuration", 
    "title": "LldpLocalPort", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "port_name":
        {
            "description": "Physical Port Number", 
            "type": "string", 
            "maxLength": 9, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "admin_status":
        {
            "description": "LLDP per port operational status.", 
            "$ref":"../../schema/common/LldpPortAdminStatus.json", 
            "default_value": "LPAS_TX_AND_RX"
        }, 
        "med_power_allocation":
        {
            "description": "LLDP MED Power allocation status", 
            "$ref":"../../schema/common/LldpMedPowerAllocation.json", 
            "default_value": "LMPA_DISABLE"
        }, 
        "notification_enable":
        {
            "description": "LLDP Enable notification", 
            "type": "boolean"
        }, 
        "med_trap_enable":
        {
            "description": "LLDP MED trap enable notification", 
            "type": "boolean"
        }, 
        "tlv_params":
        {
            "description": "TLVS Advertised", 
            "$ref":"../../schema/common/LldpTlvParams.json"
        }
    }
}
