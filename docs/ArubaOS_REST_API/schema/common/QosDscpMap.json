{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "QoS DSCP Map configuration data.", 
    "title": "QosDscpMap", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "dscp_value":
        {
            "description": "The DSCP value.", 
            "minimum": 0, 
            "maximum": 63, 
            "type": "integer"
        }, 
        "dscp_name":
        {
            "description": "The DSCP name.", 
            "type": "string", 
            "maxLength": 18, 
            "minLength": 0
        }, 
        "priority":
        {
            "description": "The priority for DSCP.", 
            "minimum": 0, 
            "maximum": 7, 
            "type": "integer"
        }
    }
}
