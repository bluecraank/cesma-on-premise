{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Schema for show command",
    "type": "object",
    "properties": {
        "SLA ID": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "SLA Type": {
            "type": "string"
        },
        "Destination Hostname": {
            "type": "string"
        },
        "DNS resolved destination IP": {
            "type": "string"
        },
        "DNS Resolution Status": {
            "type": "string"
        },
        "Source Interface": {
            "type": "string"
        },
        "Source Address": {
            "type": "string"
        },
        "Destination Address": {
            "type": "string"
        },
        "Destination Port": {
            "type": "string"
        },
        "Source Port": {
            "type": "string"
        },
        "History Size": {
            "type": "string"
        },
        "Payload Size": {
            "type": "string"
        },
        "TOS": {
            "type": "string"
        },
        "Codec Type": {
            "type": "string"
        },
        "Advantage Factor": {
            "type": "integer"
        },
        "Number of Packets per probe": {
            "type": "integer"
        },
        "Interpacket Interval (ms)": {
            "type": "integer"
        },
        "Schedule": {
            "Frequency(seconds)": {
                "type": "string"
            },
            "Life": {
                "type": "string"
            },
            "Start Time": {
                "type": "string"
            },
            "Next Scheduled Run Time": {
                "type": "string"
            }
        },
       "IP-SLA_threshold_monitor_details": {
            "type": "array",
            "items": {
                "Threshold-Monitor is": {
                    "type": "string"
                },
                "Threshold Config": {
                    "type": "string"
                },
                "Threshold Type": {
                    "type": "string"
                },
                "Upper Threshold": {
                    "type": "string"
                },
                "Lower Threshold": {
                    "type": "string"
                },
                "Action Type": {
                    "type": "string"
                }
            }
        },
        "Threshold Parameters": {
            "Positive SD Average": {
                "type": "integer"
            },
            "Positive DS Average": {
                "type": "integer"
            },
            "Average DS One-way Delay": {
                "type": "integer"
            },
            "Average RTT": {
                "type": "integer"
            },
            "Average SD One-way Delay": {
                "type": "integer"
            },
            "Average MOS": {
                "type": "integer"
            },
            "Average ICPIF": {
                "type": "integer"
            }
        }
    }
}