{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Schema for show command",
    "type": "object",
    "properties": {
        "Transmit Interval (sec)": {
            "type": "integer"
        },
        "Port Disable Timer (sec)": {
            "type": "integer"
        },
        "Loop Detected Trap": {
            "type": "string"
        },
        "Loop Protect Mode": {
            "type": "string"
        },
        "Loop Protect Enabled VLANs": {
            "type": "string"
        },
        "Loop Protection Port Status": {
            "type": "array",
            "items": {
                "Port": {
                    "type": "string"
                },
                "Loop Protect": {
                    "type": "string"
                },
                "Loop Detected": {
                    "type": "string"
                },
                "Detected on VLAN": {
                    "type": "string"
                },
                "Loop Count": {
                    "type": "integer"
                },
                "Time Since Last Loop": {
                    "type": "string"
                },
                "Rx Action": {
                    "type": "string"
                },
                "Port Status": {
                    "type": "string"
                }
            }
        }
    }
}