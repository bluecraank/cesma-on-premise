{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "QoS Policy configuration data.", 
    "title": "QosPolicy", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "id":
        {
            "description": "Unique ID of the QoS Policy.", 
            "type": "string", 
            "maxLength": 72, 
            "readonly": true, 
            "sql.auto_generated": true, 
            "sql.primary_key": true
        }, 
        "policy_name":
        {
            "description": "The QoS Policy name.", 
            "type": "string", 
            "maxLength": 64, 
            "minLength": 1, 
            "sql.not_null": true
        }, 
        "policy_type":
        {
            "description": "The QoS Policy type.", 
            "$ref":"../../schema/common/QosPolicyType.json", 
            "sql.not_null": true
        }
    }
}
