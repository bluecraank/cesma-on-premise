{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "VLAN Attributes", 
    "title": "VlanAttributes", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "appliedVlan":
        {
            "description": "Applied VLAN", 
            "type": "integer"
        }, 
        "vlanDrv":
        {
            "description": "VLAN Derivation", 
            "$ref":"../../schema/common/VlanDerivation.json"
        }, 
        "vlanTagStatus":
        {
            "description": "VLAN Tag Status", 
            "$ref":"../../schema/common/VlanTagStatus.json"
        }, 
        "vlanAssign":
        {
            "description": "VLAN Assigned Status", 
            "$ref":"../../schema/common/VlanAssignStatus.json"
        }, 
        "applied_vlan_name":
        {
            "description": "Applied VLAN Name", 
            "type": "string"
        }, 
        "applied_vlan_type":
        {
            "description": "Applied VLAN Type", 
            "$ref":"../../schema/common/PrivateVlanType.json"
        }, 
        "associated_primary_vlan_id":
        {
            "description": "Associated Primary VLAN ID", 
            "type": "integer"
        }, 
        "associated_primary_vlan_name":
        {
            "description": "Associated Primary VLAN Name", 
            "type": "string"
        }
    }
}
