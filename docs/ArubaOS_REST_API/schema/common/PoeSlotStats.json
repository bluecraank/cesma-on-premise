{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "PoE Slot Information", 
    "title": "PoeSlotStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "slot_name":
        {
            "description": "Slot name.", 
            "type": "string"
        }, 
        "operational_status":
        {
            "description": "Operational status of slot.", 
            "$ref":"../../schema/common/PoeOperationalStatus.json"
        }, 
        "maximum_power_in_watts":
        {
            "description": "Maximum Power of the PSE.", 
            "type": "integer"
        }, 
        "power_in_use_in_watts":
        {
            "description": "PSE Power in use.", 
            "type": "integer"
        }, 
        "power_usage_threshold":
        {
            "description": "Power usage threshold in percentage.", 
            "type": "integer"
        }, 
        "power_drawn_in_watts":
        {
            "description": "PD Power Drawn.", 
            "type": "integer"
        }
    }
}
