{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Private vlan configuration details.", 
    "title": "PrivateVlan", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "primary_vlan_id":
        {
            "description": "The unique ID of the VLAN. IEEE 802.1Q VLAN identifier (VID)", 
            "minimum": 1, 
            "maximum": 4094, 
            "type": "integer", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "secondary_vids":
        {
            "description": "Secondary VLAN details which are associated to primary VLAN. ( 1-Isolated VLAN + maximum of 8-Community VLANS per Primary VLAN )", 
            "type": "array", 
            "minItems": 0, 
            "maxItems": 9, 
            "items":
            {
                "$ref":"../../schema/common/SecondaryVlanList.json"
            }, 
            "sql.auto_generated": true
        }
    }
}
