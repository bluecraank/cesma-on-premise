{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "QoS Class Match configuration data.", 
    "title": "QosClassMatch", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "sequence_no":
        {
            "description": "The index of this QoS Class Match inside the list.", 
            "minimum": 1, 
            "maximum": 2147483647, 
            "type": "integer"
        }, 
        "traffic_class_id":
        {
            "description": "ID of the QoS Traffic Class to match.", 
            "type": "string", 
            "maxLength": 74, 
            "minLength": 1
        }, 
        "entry_type":
        {
            "description": "QoS Traffic Class entry type.", 
            "$ref":"../../schema/common/QosTrafficClassEntryType.json", 
            "sql.not_null": true
        }, 
        "traffic_match":
        {
            "description": "Traffic matching criteria.", 
            "$ref":"../../schema/common/TrafficMatch.json", 
            "sql.not_null": true
        }, 
        "dscp_value":
        {
            "description": "QoS DSCP value to match. dscp_value will override tos and precedence. Only one of TOS and PRECEDENCE or DSCP may be set in an ACE at a time.", 
            "minimum": 0, 
            "maximum": 63, 
            "type": "integer"
        }
    }
}
