{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Wraps the parameters to execute a file transfer.", 
    "title": "FileTransferCommand", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "file_type":
        {
            "description": "The file type to be transferred.", 
            "$ref":"../../schema/common/FileTransferType.json", 
            "sql.not_null": true
        }, 
        "action":
        {
            "description": "The action to be performed.", 
            "$ref":"../../schema/common/FileTransferAction.json", 
            "sql.not_null": true
        }, 
        "url":
        {
            "description": "The location in which the action will take place.", 
            "type": "string", 
            "sql.not_null": true
        }, 
        "boot_image":
        {
            "description": "The location where the firmware image is loaded on the device.", 
            "$ref":"../../schema/common/BootImage.json", 
            "default_value": "BI_PRIMARY_IMAGE"
        }, 
        "show_tech_option":
        {
            "description": "Option for the show-tech file transfer type.", 
            "$ref":"../../schema/common/ShowTechOption.json", 
            "default_value": "STO_BASIC"
        }
    }
}
