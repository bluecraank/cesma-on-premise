{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "RADIUS Profile configuration data.", 
    "title": "RadiusProfile", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "retry_interval":
        {
            "description": "The RADIUS server retry interval.", 
            "minimum": 1, 
            "maximum": 15, 
            "type": "integer", 
            "default_value": 5
        }, 
        "retransmit_attempts":
        {
            "description": "The RADIUS server retransmit attempts.", 
            "minimum": 1, 
            "maximum": 5, 
            "type": "integer", 
            "default_value": 3
        }, 
        "dead_time":
        {
            "description": "The RADIUS server dead_time. dead_time cannot set when is_tracking_enabled is true. Input dead_time as null to reset the value. dead_time is indicated as null instead of '0' in CLI", 
            "minimum": 1, 
            "maximum": 1440, 
            "type": "integer"
        }, 
        "key":
        {
            "description": "The RADIUS server key. Input key as empty string to reset the value.", 
            "type": "string", 
            "maxLength": 32, 
            "minLength": 0
        }, 
        "dyn_autz_port":
        {
            "description": "The RADIUS dyn_autz_port.", 
            "minimum": 1024, 
            "maximum": 49151, 
            "type": "integer", 
            "default_value": 3799
        }, 
        "tracking_uname":
        {
            "description": "The RADIUS tracking_uname.", 
            "type": "string", 
            "maxLength": 64, 
            "minLength": 1, 
            "default_value": "radius-tracking-user"
        }, 
        "is_tracking_enabled":
        {
            "description": "The RADIUS server is_tracking_enabled . is_tracking_enabled cannot set to true when dead_time is configured.", 
            "type": "boolean"
        }, 
        "cppm_details":
        {
            "description": "Username and password combination of CPPM which is used to login to CPPM to download user roles.", 
            "$ref":"../../schema/common/CppmDetails.json"
        }, 
        "source_ip_select":
        {
            "description": "RADIUS Server source IP selection", 
            "type": "string", 
            "default_value": "Outgoing Interface"
        }
    }
}
