{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "ISL Port configuration details.", 
    "title": "IslPorts", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "primary_vlan_id":
        {
            "description": "The unique ID of the VLAN. IEEE 802.1Q VLAN identifier (VID)", 
            "minimum": 1, 
            "maximum": 4094, 
            "type": "integer", 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "isl_port_id":
        {
            "description": "List of isl port-ids.", 
            "type": "array", 
            "items":
            {
                "type": "string", 
                "maxLength": 9, 
                "minLength": 1
            }, 
            "static_url": ["/system/status/switch#/blades/data_ports/port_name"], 
            "dynamic_url": ["/system/status/switch#/blades/expansion_modules/data_ports/port_name"], 
            "sql.auto_generated": true
        }
    }
}
