{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Tftp Client configuration data.", 
    "title": "TftpClient", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "is_tftp_client_enabled":
        {
            "description": "TFTP Client status", 
            "type": "boolean", 
            "default_value": true, 
            "sql.not_null": true
        }, 
        "tftp_client_block_size":
        {
            "description": "TFTP transfer block size tftp_client_block_size is valid only when is_tftp_client_enabled is true", 
            "minimum": 512, 
            "maximum": 1416, 
            "type": "integer", 
            "default_value": 1416
        }
    }
}
