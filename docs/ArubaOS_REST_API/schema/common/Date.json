{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Date configuration data.", 
    "title": "Date", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "year":
        {
            "description": "Year (YYYY)", 
            "minimum": 1990, 
            "type": "integer", 
            "sql.not_null": true
        }, 
        "month":
        {
            "description": "Month (MM)", 
            "$ref":"../../schema/common/Month.json", 
            "sql.not_null": true
        }, 
        "day_of_month":
        {
            "description": "Day of Month (DD)", 
            "minimum": 1, 
            "maximum": 31, 
            "type": "integer", 
            "sql.not_null": true
        }
    }
}
