{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Information about the LLDP Remote Device", 
    "title": "LldpRemoteDevice", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "local_port":
        {
            "description": "Local Port", 
            "type": "string", 
            "maxLength": 9
        }, 
        "chassis_type":
        {
            "description": "Chassis Type", 
            "$ref":"../../schema/common/LldpChassisType.json"
        }, 
        "chassis_id":
        {
            "description": "Chassis ID", 
            "type": "string", 
            "maxLength": 16
        }, 
        "port_type":
        {
            "description": "Port Type", 
            "$ref":"../../schema/common/LldpPortType.json"
        }, 
        "port_id":
        {
            "description": "Port ID", 
            "type": "integer"
        }, 
        "port_description":
        {
            "description": "Port Description", 
            "type": "string", 
            "maxLength": 9
        }, 
        "system_name":
        {
            "description": "System Name", 
            "type": "string", 
            "maxLength": 32
        }, 
        "system_description":
        {
            "description": "System Description", 
            "type": "string", 
            "maxLength": 256
        }, 
        "pvid":
        {
            "description": "pvid", 
            "type": "integer"
        }, 
        "capabilities_supported":
        {
            "description": "System Capabilities Supported", 
            "$ref":"../../schema/common/LldpSystemCapabilities.json"
        }, 
        "capabilities_enabled":
        {
            "description": "System Capabilities Enabled", 
            "$ref":"../../schema/common/LldpSystemCapabilities.json"
        }, 
        "remote_management_address":
        {
            "description": "Remote Management Address", 
            "$ref":"../../schema/common/ManagementAddress.json"
        }, 
        "poe_plus_info":
        {
            "description": "Poe Plus Information Detail, NA for oobm port.", 
            "$ref":"../../schema/common/LldpPoePlusInfo.json"
        }
    }
}
