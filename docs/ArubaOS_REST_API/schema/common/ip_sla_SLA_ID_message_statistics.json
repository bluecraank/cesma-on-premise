{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Schema for show command",
    "type": "object",
    "properties": {
        "SLA ID": {
            "type": "string"
        },
        "Status": {
            "type": "string"
        },
        "SLA Type": {
            "type": "string"
        },
        "Destination Address": {
            "type": "string"
        },
        "Source Interface": {
            "type": "string"
        },
        "Destination Port": {
            "type": "string"
        },
        "Source Port": {
            "type": "string"
        },
        "History Size": {
            "type": "string"
        },
        "Payload Size": {
            "type": "string"
        },
        "TOS": {
            "type": "string"
        },
		"Destination Hostname": {
            "type": "string"
        },
        "DNS resolved destination IP": {
            "type": "string"
        },
        "DNS Resolution Status": {
            "type": "string"
        },
        "Messages": {
            "Destination Address Unreachable": {
                "type": "string"
            },
            "Probes Skipped Awaiting DNS Resolution": {
                "type": "string"
            },
            "DNS Resolution Failed": {
                "type": "string"
            },
            "No Route to Target": {
                "type": "string"
            },
            "Internal Error": {
                "type": "string"
            },
            "Local Interface is Down": {
                "type": "string"
            },
            "No Response from Target": {
                "type": "string"
            },
            "Successful Probes Sent": {
                "type": "string"
            },
            "Probe Response received": {
                "type": "string"
            },
            "Possibly Tail Dropped": {
                "type": "string"
            },
			"Unable to connect to remote host": {
                "type": "string"
            },
            "Successful Connections": {
                "type": "string"
            }
        },
        "Count of Threshold hits": {
            "RTT": {
                "type": "string"
            },
            "srcTodstTime": {
                "type": "string"
            },
            "dstTosrcTime": {
                "type": "string"
            },
            "packetLoss": {
                "type": "string"
            }
        }
    }
}