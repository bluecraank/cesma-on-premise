{
  "title": "proxyExceptionIpIpAddr",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "ip-addr": {
      "properties": {
        "ip-addr_value": {
          "type": "string",
          "format": "ipv4",
          "description": "Configures proxy exception IPv4 address."
        },
        "ip-addr_subnet_value": {
          "type": "string",
          "pattern": "^(((255[.]{1}){3}(255|254|252|248|240|224|192|128|0))|((255[.]{1}){2}(255|254|252|248|240|224|192|128|0)[.]{1}0)|((255[.]{1})(255|254|252|248|240|224|192|128|0)([.]{1}0){2})|((255|254|252|248|240|224|192|128)([.]{1}0){3}))$"
        }
      },
      "required": [
        "ip-addr_value",
        "ip-addr_subnet_value"
      ]
    },
    "uri": {
      "type": "string",
      "description": "The URI of the configuration entity",
      "readOnly": true
    }
  },
  "required": [
    "ip-addr"
  ]
}