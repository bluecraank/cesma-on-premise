{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Wraps the parameters to enable/disable debug log filters and debug destinations.", 
    "title": "DebugLogFilterCommand", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "debug_destination_to_enable":
        {
            "description": "The set of destination to enable.", 
            "type": "array", 
            "minItems": 0, 
            "maxItems": 2, 
            "items":
            {
                "$ref": "../../schema/common/DebugDestination.json"
            }
        }, 
        "debug_destination_to_disable":
        {
            "description": "The set of destination to disable.", 
            "type": "array", 
            "minItems": 0, 
            "maxItems": 2, 
            "items":
            {
                "$ref": "../../schema/common/DebugDestination.json"
            }
        }, 
        "filter_to_enable":
        {
            "description": "The set of filters to enable.", 
            "type": "array", 
            "minItems": 0, 
            "maxItems": 185, 
            "items":
            {
                "$ref": "../../schema/common/DebugLogFilter.json"
            }
        }, 
        "filter_to_disable":
        {
            "description": "The set of filters to disable.", 
            "type": "array", 
            "minItems": 0, 
            "maxItems": 185, 
            "items":
            {
                "$ref": "../../schema/common/DebugLogFilter.json"
            }
        }
    }
}
