{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Schema for show command",
    "type": "object",
    "properties": {
        "NTP_Server_Address_Information": {
            "type": "array",
            "items": {
                "Server address": {
                    "type": "string"
                }
            }
        },
        "NTP_Server_Name_Information": {
            "type": "array",
            "items": {
                "Server Name": {
                    "type": "string"
                },
                "Resolved Address": {
                    "type": "string"
                },
                "Server Status": {
                    "type": "string"
                }
            }
        }
    }
}