{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "", 
    "title": "RadiusAccountingStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "nas_id":
        {
            "description": "NAS-Identifier", 
            "type": "string", 
            "maxLength": 32, 
            "minLength": 1
        }, 
        "invalid_server_addresses":
        {
            "description": "Number of Accounting-Response received from unknown addresses.These values are calculated at runtime.", 
            "type": "integer", 
            "default_value": 0, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "address":
        {
            "description": "The Server's IP address.", 
            "$ref":"../../schema/common/IpAddress.json"
        }, 
        "udp_port":
        {
            "description": "The RADIUS server accounting port.", 
            "minimum": 1, 
            "maximum": 65535, 
            "type": "integer", 
            "default_value": 1813
        }, 
        "timeouts":
        {
            "description": "number of accounting timeouts. These values are calculated at runtime.", 
            "type": "integer", 
            "default_value": 0, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "requests":
        {
            "description": "The number of RADIUS Accounting-Request sent to server. These values are calculated at runtime.", 
            "type": "integer", 
            "default_value": 0, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "responses":
        {
            "description": "The number of RADIUS Accounting-Response recieved from server. These values are calculated at runtime.", 
            "type": "integer", 
            "default_value": 0, 
            "readonly": true, 
            "sql.auto_generated": true
        }
    }
}
