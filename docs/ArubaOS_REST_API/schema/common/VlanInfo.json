{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "Vlan details", 
    "title": "VlanInfo", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "vlan_id":
        {
            "description": "Vlan ID", 
            "minimum": 1, 
            "maximum": 4094, 
            "type": "integer"
        }, 
        "vlan_name":
        {
            "description": "Vlan Name", 
            "type": "string", 
            "maxLength": 32, 
            "minLength": 1
        }
    }
}
