{
    "$schema": "http://json-schema.org/draft-04/schema#", 
    "description": "", 
    "title": "RadiusAuthStats", 
    "type": "object", 
    "properties":
    {
        "uri":
        {
            "description": "The URI of the configuration entity", 
            "type": "string", 
            "readonly": true
        }, 
        "nas_id":
        {
            "description": "NAS-Identifier", 
            "type": "string", 
            "maxLength": 32, 
            "minLength": 1
        }, 
        "invalid_server_addresses":
        {
            "description": "Number of Access-Response received from unknown addresses. These values are calculated at runtime.", 
            "type": "integer", 
            "default_value": 0, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "address":
        {
            "description": "The Server's IP address.", 
            "$ref":"../../schema/common/IpAddress.json"
        }, 
        "udp_port":
        {
            "description": "The RADIUS server authentication port.", 
            "minimum": 1, 
            "maximum": 65535, 
            "type": "integer", 
            "default_value": 1812
        }, 
        "timeouts":
        {
            "description": "number of authentication timeouts. These values are calculated at runtime.", 
            "type": "integer", 
            "default_value": 0, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "requests":
        {
            "description": "The number of RADIUS Access-Request sent to server. These values are calculated at runtime.", 
            "type": "integer", 
            "default_value": 0, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "challenges":
        {
            "description": "The number of RADIUS Access-Challenge recieved from server. These values are calculated at runtime.", 
            "type": "integer", 
            "default_value": 0, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "accepts":
        {
            "description": "The number of RADIUS Access-Accept recieved from server. These values are calculated at runtime.", 
            "type": "integer", 
            "default_value": 0, 
            "readonly": true, 
            "sql.auto_generated": true
        }, 
        "rejects":
        {
            "description": "The number of RADIUS Access-Reject recieved from server. These values are calculated at runtime.", 
            "type": "integer", 
            "default_value": 0, 
            "readonly": true, 
            "sql.auto_generated": true
        }
    }
}
